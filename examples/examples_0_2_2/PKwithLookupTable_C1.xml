<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.pharmml.org/2013/03/PharmML"
    xsi:schemaLocation="http://www.pharmml.org/2013/03/PharmML http://www.pharmml.org/2013/03/PharmML"
    xmlns:math="http://www.pharmml.org/2013/03/Maths"
    xmlns:ct="http://www.pharmml.org/2013/03/CommonTypes"
    xmlns:ds="http://www.pharmml.org/2013/08/Dataset"
    xmlns:mdef="http://www.pharmml.org/2013/03/ModelDefinition"
    xmlns:mstep="http://www.pharmml.org/2013/03/ModellingSteps"
    xmlns:mml="http://www.pharmml.org/2013/03/PharmML"
    xmlns:un="http://www.uncertml.org/3.0"
    writtenVersion="0.2.2" id="i1">
    
    <ct:Name>Testing PK with look-up table</ct:Name>
    
    <!-- INDEPENDENT VARIABLE -->
    <IndependentVariable symbId="t"/>
    
    <!-- FUNCTION DEFINITION -->
    <FunctionDefinition xmlns="http://www.pharmml.org/2013/03/CommonTypes"
        symbId="constantErrorModel" symbolType="real">
        <FunctionArgument symbId="a" symbolType="real"/>
        <Definition>
            <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                <ct:SymbRef symbIdRef="a"/>
            </Equation>
        </Definition>
    </FunctionDefinition>

    <!-- MODEL DEFINITION -->
    <ModelDefinition xmlns="http://www.pharmml.org/2013/03/ModelDefinition">

        <!-- VARIABILITY MODEL -->
        <VariabilityModel blkId="randEffect" type="parameterVariability">
            <Level symbId="indiv">
                <ct:Name>Individual Variability</ct:Name>
            </Level>
        </VariabilityModel>

        <VariabilityModel blkId="obsErr" type="residualError">
            <Level symbId="residual">
                <ct:Name>Residual Error</ct:Name>
            </Level>
        </VariabilityModel>

        <!-- COVARIATE MODEL -->
        <CovariateModel blkId="cm1">
            <SimpleParameter symbId="pop_W"/>
            <SimpleParameter symbId="omega_W"/>
            <Covariate symbId="W">
                <Continuous>
                    <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                        <mean>
                            <var varId="pop_W"/>
                        </mean>
                        <variance>
                            <var varId="omega_W"/>
                        </variance>
                    </NormalDistribution>
                    <Transformation>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <Uniop op="log">
                                <Binop op="divide">
                                    <ct:SymbRef symbIdRef="W"/>
                                    <ct:Real>70.0</ct:Real>
                                </Binop>
                            </Uniop>
                        </Equation>
                    </Transformation>
                    <ct:Interpolation>
                        <ct:Algorithm>linear</ct:Algorithm>
                        <ct:InterpIndepVar>
                            <ct:SymbRef symbIdRef="t"/>
                        </ct:InterpIndepVar>
                    </ct:Interpolation>
                </Continuous>
            </Covariate>
        </CovariateModel>
        
        <!-- PARAMETER MODEL -->
        <ParameterModel blkId="pm1">

            <!-- Rin -->
            <SimpleParameter symbId="pop_Rin"/>
            <SimpleParameter symbId="omega_Rin"/>
            <RandomVariable symbId="eta_Rin">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="randEffect" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Rin"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="Rin">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <ct:SymbRef symbIdRef="pop_R_in"/>
                            <Uniop op="exp">
                                <ct:SymbRef symbIdRef="eta_R_in"/>
                            </Uniop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            <!-- kout -->
            <SimpleParameter symbId="pop_kout"/>
            <SimpleParameter symbId="omega_kout"/>
            <RandomVariable symbId="eta_kout">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="randEffect" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_kout"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="kout">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <GeneralCovariate>
                        <ct:Assign>
                            <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                                <Binop op="plus">
                                    <Uniop op="log">
                                        <ct:SymbRef symbIdRef="pop_kout"/>
                                    </Uniop>
                                    <ct:SymbRef symbIdRef="eta_kout"/>
                                </Binop>
                            </Equation>
                        </ct:Assign>
                    </GeneralCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_kout"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            <!-- IC50 -->
            <SimpleParameter symbId="pop_IC50"/>
            <SimpleParameter symbId="omega_IC50"/>
            <RandomVariable symbId="eta_IC50">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="randEffect" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_IC50"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="IC50">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_IC50"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_IC50"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <Correlation>
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="randEffect" symbIdRef="indiv"/>
                </ct:VariabilityReference>                
                <Matrix matrixType="CovarianceMatrix">
                    <ct:RowNames>
                        <ct:SymbRef symbIdRef="PPV_CL"/>
                        <ct:SymbRef symbIdRef="PPV_V"/>
                        <ct:SymbRef symbIdRef="PPV_EMAX"/>
                        <ct:SymbRef symbIdRef="PPV_C50"/>
                    </ct:RowNames>
                    <ct:MatrixRow>
                        <ct:Real>0.1</ct:Real>
                    </ct:MatrixRow>
                    <ct:MatrixRow>
                        <ct:Real>0.01</ct:Real><ct:Real>0.1</ct:Real>
                    </ct:MatrixRow>
                    <ct:MatrixRow>
                        <ct:Real>0</ct:Real><ct:Real>0.01</ct:Real><ct:Real>0.1</ct:Real>
                    </ct:MatrixRow>
                    <ct:MatrixRow>
                        <ct:Real>0</ct:Real><ct:Real>0</ct:Real><ct:Real>0.01</ct:Real><ct:Real>0.01</ct:Real>
                    </ct:MatrixRow>
                </Matrix>            
            </Correlation>
            
            <Correlation>
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <Matrix matrixType="CorrelationMatrix">
                    <ct:RowNames><ct:SymbRef symbIdRef="ka"/><ct:SymbRef symbIdRef="V"/></ct:RowNames>
                    <ct:ColumnNames><ct:SymbRef symbIdRef="ka"/><ct:SymbRef symbIdRef="V"/></ct:ColumnNames>
                    <ct:MatrixRow>
                        <ct:SymbRef symbIdRef="ds"/>
                    </ct:MatrixRow>
                </Matrix>
            </Correlation>
            
        </ParameterModel>
        
        <!-- STRUCTURAL MODEL -->
        <StructuralModel blkId="sm1">
            
            <!-- TARGET FOR LOOKUP DATA REFERENCE -->
            <ct:Variable symbolType="real" symbId="Cc">
                <ct:Assign>
                    <ct:Interpolation>
                        <ct:Algorithm>linear</ct:Algorithm>
                        <ct:InterpIndepVar>
                            <ct:SymbRef symbIdRef="t"/>
                        </ct:InterpIndepVar>
                    </ct:Interpolation>
                </ct:Assign>
            </ct:Variable>

            <ct:DerivativeVariable symbId="E" symbolType="real">
                <ct:Description>Effect</ct:Description>
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="minus">
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="Rin"/>
                                <Binop op="minus">
                                    <ct:Real>1</ct:Real>
                                    <Binop op="divide">
                                        <Binop op="times">
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="Imax"/>
                                            <ct:SymbRef symbIdRef="Cc"/>
                                        </Binop>
                                        <Binop op="plus">
                                            <ct:SymbRef symbIdRef="Cc"/>
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="IC50"/>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="kout"/>
                                <ct:SymbRef symbIdRef="E"/>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:DerivativeVariable>
        </StructuralModel>

        <!-- OBSERVATION MODEL -->
        <ObservationModel blkId="om1">
            <SimpleParameter symbId="a"/>
            <RandomVariable symbId="epsilon_E">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="obsErr" symbIdRef="residual"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="sigma_E"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <General symbId="E_obs">
                <ct:Assign>
                    <math:Equation>
                        <math:Binop op="plus">
                            <ct:SymbRef blkIdRef="sm1" symbIdRef="E"/>
                            <math:Binop op="times">
                                <math:FunctionCall>
                                    <ct:SymbRef symbIdRef="constantErrorModel"/>
                                    <math:FunctionArgument symbId="a">
                                        <ct:SymbRef symbIdRef="a"/>
                                    </math:FunctionArgument>
                                </math:FunctionCall>
                                <ct:SymbRef symbIdRef="epsilon_E"/>
                            </math:Binop>
                        </math:Binop>
                    </math:Equation>
                </ct:Assign>
            </General>
        </ObservationModel>
    </ModelDefinition>
    
    <!-- TRIAL DESIGN -->
    <TrialDesign xmlns="http://www.pharmml.org/2013/03/TrialDesign">
        
        <!-- STRUCTURE -->
        <Structure>
            <Epoch oid="e1">
                <Start>
                    <ct:Real>0</ct:Real>
                </Start>
                <End>
                    <ct:Real>300</ct:Real>
                </End>
                <Order>1</Order>
            </Epoch>
            <Arm oid="a1"/>
            <Cell oid="c1">
                <EpochRef oidRef="e1" />
                <ArmRef oidRef="a1"/>
                <SegmentRef oidRef="ta"/>
            </Cell>
            <Segment oid="ta">
                <ActivityRef oidRef="d1"/>
            </Segment>
            <Activity oid="d1">

                <LookupTable>
                    <!-- MAPPING -->
                    <ColumnMapping>
                        <ds:ColumnRef columnIdRef="TIME"/>
                        <ct:SymbRef symbIdRef="t"/>
                    </ColumnMapping>
                    
                    <Target inputTarget="variable">
                        <ColumnMapping> 
                            <ds:ColumnRef columnIdRef="Cc"/>
                            <ct:SymbRef blkIdRef="sm1" symbIdRef="Cc"/>
                        </ColumnMapping>
                    </Target>
                    
                    <!-- DATASET -->
                    <ds:DataSet>
                        <ds:Definition>
                            <ds:Column columnId="ID" columnType="id" valueType="id" columnNum="1"/>
                            <ds:Column columnId="TIME" columnType="time" valueType="real" columnNum="2"/>
<!--                            <ds:Column columnId="EPOCH" columnType="epoch" valueType="real" columnNum="2"/>-->
                            <ds:Column columnId="Cc" columnType="dv" valueType="real" columnNum="3"/>
                        </ds:Definition>
                        <ds:ImportData oid="importData">
                            <ds:name>Cc_lookupTable</ds:name>
                            <ds:url>file:///../../datasets/</ds:url>
                            <ds:format>CSV</ds:format>
                            <ds:delimiter>COMMA</ds:delimiter>
                        </ds:ImportData>
                    </ds:DataSet>
                </LookupTable>
            </Activity>
        </Structure>
        
        
        <!-- POPULATION -->
        <Population>
            <ct:VariabilityReference>
                <ct:SymbRef blkIdRef="randEffect" symbIdRef="indiv"/>
            </ct:VariabilityReference>
            
            <ColumnMapping>
                <ds:ColumnRef columnIdRef="WEIGHT"/>
                <ct:SymbRef blkIdRef="cm1" symbIdRef="W"/>
            </ColumnMapping>
            
            <ds:DataSet>
                <ds:Definition>
                    <ds:Column columnId="ID" columnType="id" valueType="id" columnNum="1"/>
                    <ds:Column columnId="ARM" columnType="arm" valueType="id" columnNum="2"/>
                    <ds:Column columnId="WEIGHT" columnType="covariate" valueType="real" columnNum="3"/>
                </ds:Definition>
                <ds:Table>
                    <ds:Row><ct:Id>i</ct:Id><ct:Id>arm1</ct:Id><ct:Real>20</ct:Real></ds:Row>
                </ds:Table>
            </ds:DataSet>
        </Population>
    </TrialDesign>
    
    <!-- MODELLING STEPS -->
    <ModellingSteps xmlns="http://www.pharmml.org/2013/03/ModellingSteps">
        
        
        <EstimationStep oid="dasd">
            
            <ParametersToEstimate>
                <ParameterEstimation>
                    <ct:SymbRef symbIdRef="sad"></ct:SymbRef>
                </ParameterEstimation>
            </ParametersToEstimate>
            <Operation order="1" opType="estIndiv"></Operation>
        </EstimationStep>
        
        <SimulationStep oid="s1">
            
<!--            <TargetToolReference>
                <ct:OidRef oidRef="NMoid"/>
            </TargetToolReference>-->
                        
            <ct:VariableAssignment>
                <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_Imax"/>
                <ct:Assign>
                    <ct:Real>0.9</ct:Real>
                </ct:Assign>
            </ct:VariableAssignment>
            <ct:VariableAssignment>
                <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_Imax"/>
                <ct:Assign>
                    <ct:Real>2</ct:Real>
                </ct:Assign>
            </ct:VariableAssignment>
            <ct:VariableAssignment>
                <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_IC50"/>
                <ct:Assign>
                    <ct:Real>0.4</ct:Real>
                </ct:Assign>
            </ct:VariableAssignment>
            <ct:VariableAssignment>
                <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_IC50"/>
                <ct:Assign>
                    <ct:Real>0.4</ct:Real>
                </ct:Assign>
            </ct:VariableAssignment>
            <ct:VariableAssignment>
                <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_kout"/>
                <ct:Assign>
                    <ct:Real>0.05</ct:Real>
                </ct:Assign>
            </ct:VariableAssignment>
            <ct:VariableAssignment>
                <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_kout"/>
                <ct:Assign>
                    <ct:Real>0.05</ct:Real>
                </ct:Assign>
            </ct:VariableAssignment>
            <ct:VariableAssignment>
                <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_Rin"/>
                <ct:Assign>
                    <ct:Real>5</ct:Real>
                </ct:Assign>
            </ct:VariableAssignment>
            <ct:VariableAssignment>
                <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_Rin"/>
                <ct:Assign>
                    <ct:Real>0.05</ct:Real>
                </ct:Assign>
            </ct:VariableAssignment>

            <Observations>
                <Timepoints>
                    <ct:Sequence>
                        <ct:Begin><ct:Int>0</ct:Int></ct:Begin>
                        <ct:StepSize><ct:Int>24</ct:Int></ct:StepSize>
                        <ct:End><ct:Int>288</ct:Int></ct:End>
                    </ct:Sequence>
                </Timepoints>
                <Continuous>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="E_obs"/>
                </Continuous>
            </Observations>

            <Observations>
                <Timepoints>
                    <ct:Sequence>
                        <ct:Begin><ct:Int>0</ct:Int></ct:Begin>
                        <ct:StepSize><ct:Int>24</ct:Int></ct:StepSize>
                        <ct:End><ct:Int>288</ct:Int></ct:End>
                    </ct:Sequence>
                </Timepoints>
                <Continuous>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="C"/>
                </Continuous>
            </Observations>
            
            <Operation order="1" opType="simulatePK">
                <Property name="toolName">
                    <ct:Assign>
                        <ct:String>MATLAB</ct:String>
                    </ct:Assign>
                </Property>
                <Property name="versionNumber">
                    <ct:Assign>
                        <ct:String>R2013b</ct:String>
                    </ct:Assign>
                </Property>
            </Operation>
            
            <Operation order="2" opType="simulatePD">
                <Property name="toolName">
                    <ct:Assign>
                        <ct:String>R</ct:String>
                    </ct:Assign>
                </Property>
            </Operation>
            
        </SimulationStep>
        
        <!-- STEP DEPENDENCIES -->
        <StepDependencies>
            <Step>
                <ct:OidRef oidRef="s1"/>
            </Step>
        </StepDependencies>
    </ModellingSteps>
</PharmML>