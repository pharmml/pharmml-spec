    <IndependentVariable symbId="t"/>
    
    <FunctionDefinition xmlns="http://www.pharmml.org/2013/03/CommonTypes"
        symbId="constantErrorModel" symbolType="real">
        <FunctionArgument symbId="a" symbolType="real"/>
        <Definition>
            <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                <ct:SymbRef symbIdRef="a"/>
            </Equation>
        </Definition>
    </FunctionDefinition>
    
    <FunctionDefinition xmlns="http://www.pharmml.org/2013/03/CommonTypes"
        symbId="combinedErrorModel" symbolType="real">
        <FunctionArgument symbId="a" symbolType="real"/>
        <FunctionArgument symbId="b" symbolType="real"/>
        <FunctionArgument symbId="f" symbolType="real"/>
        <Definition>
            <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                <Binop op="plus">
                    <ct:SymbRef symbIdRef="a"/>
                    <Binop op="times">
                        <ct:SymbRef symbIdRef="b"/>
                        <ct:SymbRef symbIdRef="f"/>
                    </Binop>
                </Binop>
            </Equation>
        </Definition>
    </FunctionDefinition>
    
    <ModelDefinition xmlns="http://www.pharmml.org/2013/03/ModelDefinition">
        <VariabilityModel blkId="model" type="parameterVariability">
            <Level symbId="country">
                <ct:Name>Country</ct:Name>
            </Level>
            <Level symbId="centre">
                <ct:Name>Study Centre</ct:Name>
                <ParentLevel>
                    <ct:SymbRef symbIdRef="country"/>
                </ParentLevel>
            </Level>
            <Level symbId="indiv">
                <ct:Name>Individual Variability</ct:Name>
                <ParentLevel>
                    <ct:SymbRef symbIdRef="centre"/>
                </ParentLevel>
            </Level>
        </VariabilityModel>
        
        <VariabilityModel blkId="obsErr" type="residualError">
            <Level symbId="residual">
                <ct:Name>Residual Error</ct:Name>
                <ParentLevel>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ParentLevel>
            </Level>
        </VariabilityModel>
        
        <CovariateModel blkId="cm1">
            <SimpleParameter symbId="pop_W"/>
            <SimpleParameter symbId="omega_W"/>
            <Covariate symbId="W">
                <Continuous>
                    <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                        <mean>
                            <var varId="pop_W"/>
                        </mean>
                        <variance>
                            <var varId="omega_W"/>
                        </variance>
                    </NormalDistribution>
                    <Transformation>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <Uniop op="log">
                                <Binop op="divide">
                                    <ct:SymbRef symbIdRef="W"/>
                                    <ct:Real>70.0</ct:Real>
                                </Binop>
                            </Uniop>
                        </Equation>
                    </Transformation>
                </Continuous>
            </Covariate>
        </CovariateModel>
        
        <ParameterModel blkId="pm1">
            <!-- V -->
            <SimpleParameter symbId="beta_V">
            </SimpleParameter>
            <SimpleParameter symbId="pop_V">
            </SimpleParameter>
            <SimpleParameter symbId="omega_V">
            </SimpleParameter>
            <RandomVariable symbId="eta_V">
                <ct:VariabilityReference>
                   <ct:SymbRef blkIdRef="model" symbIdRef="level"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_V"/></stddev>
                </NormalDistribution>                            
            </RandomVariable>
            <IndividualParameter symbId="V">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_V"></ct:SymbRef>
                            </ct:Assign>
                        </PopulationParameter>
                        <Covariate>
                            <ct:SymbRef blkIdRef="cm1" symbIdRef="W"></ct:SymbRef>
                            <FixedEffect>
                                <ct:SymbRef symbIdRef="beta_V"></ct:SymbRef>
                            </FixedEffect>
                        </Covariate>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_V"></ct:SymbRef>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            <!-- ka -->
            <SimpleParameter symbId="pop_ka"/>
            <SimpleParameter symbId="omega_ka"/>
            <RandomVariable symbId="eta_ka">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"></ct:SymbRef>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_ka"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="ka">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_ka"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_ka"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            <!-- Imax -->
            <SimpleParameter symbId="pop_Imax"/>
            <SimpleParameter symbId="omega_Imax"/>
            <RandomVariable symbId="eta_Imax">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Imax"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="Imax">
                <GaussianModel>
                    <Transformation>logit</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign><ct:SymbRef symbIdRef="pop_Imax"></ct:SymbRef></ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_Imax"></ct:SymbRef>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            <!-- CL -->
            <SimpleParameter symbId="pop_Cl"/>
            <SimpleParameter symbId="omega_Cl"/>
            <SimpleParameter symbId="beta_Cl"/>
            <SimpleParameter symbId="rho_V_Cl"/>
            <RandomVariable symbId="eta_Cl">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Cl"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="Cl">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign><ct:SymbRef symbIdRef="pop_Cl"></ct:SymbRef></ct:Assign>
                        </PopulationParameter>
                        <Covariate>
                            <ct:SymbRef blkIdRef="cm1" symbIdRef="W"></ct:SymbRef>
                            <FixedEffect>
                                <ct:SymbRef symbIdRef="beta_Cl"/>
                            </FixedEffect>
                        </Covariate>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_Cl"></ct:SymbRef>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            <!-- Rin -->
            <SimpleParameter symbId="pop_Rin"/>
            <SimpleParameter symbId="omega_Rin"/>
            <RandomVariable symbId="eta_Rin">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Rin"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="Rin">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <ct:SymbRef symbIdRef="pop_Rin"/>
                            <Uniop op="exp">
                                <ct:SymbRef symbIdRef="eta_Rin"/>
                            </Uniop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            <!-- kout -->
            <SimpleParameter symbId="pop_kout"/>
            <SimpleParameter symbId="omega_kout"/>
            <RandomVariable symbId="eta_kout">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_kout"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <SimpleParameter symbId="gamma1_kout"/>
            <RandomVariable symbId="kappa1_kout">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="centre"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/NormalDistribution">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="gamma1_kout"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <SimpleParameter symbId="gamma2_kout"/>
            <RandomVariable symbId="kappa2_kout">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="country"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/NormalDistribution">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="gamma2_kout"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="kout">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_kout"></ct:SymbRef>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_kout"/>
                        <ct:SymbRef symbIdRef="kappa1_kout"/>
                        <ct:SymbRef symbIdRef="kappa2_kout"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            <!-- IC50 -->
            <SimpleParameter symbId="pop_IC50"/>
            <SimpleParameter symbId="omega_IC50"/>
            <RandomVariable symbId="eta_IC50">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_IC50"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="IC50">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_IC50"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_IC50"></ct:SymbRef>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            <Correlation>
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"></ct:SymbRef>
                </ct:VariabilityReference>
                <Pairwise>
                    <RandomVariable1>
                        <ct:SymbRef symbIdRef="eta_V"/>
                    </RandomVariable1>
                    <RandomVariable2>
                        <ct:SymbRef symbIdRef="eta_Cl"></ct:SymbRef>
                    </RandomVariable2>
                    <CorrelationCoefficient>
                        <ct:SymbRef symbIdRef="rho_V_Cl"/>
                    </CorrelationCoefficient>
                </Pairwise>
            </Correlation>
        </ParameterModel>
        
        <!-- STRUCTURAL MODEL -->
        <StructuralModel blkId="main">
            <ct:Variable symbId="k" symbolType="real">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="divide">
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="Cl"/>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="V"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:Variable>
            <!-- Ad -->
            <ct:DerivativeVariable symbId="Ad" symbolType="real">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <Uniop op="minus">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="ka"/>
                            </Uniop>
                            <ct:SymbRef symbIdRef="Ad"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="t"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign><ct:Real>0</ct:Real></ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            <!-- Ac -->
            <ct:DerivativeVariable symbId="Ac" symbolType="real">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="minus">
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="ka"/>
                                <ct:SymbRef symbIdRef="Ad"/>
                            </Binop>
                            <Binop op="times">
                                <ct:SymbRef symbIdRef="k"/>
                                <ct:SymbRef symbIdRef="Ac"/>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="t"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign><ct:Real>0</ct:Real></ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            <!-- E -->
            <ct:DerivativeVariable symbId="E" symbolType="real">
                <ct:Description>PCA</ct:Description>
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="minus">
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="Rin"/>
                                <Binop op="minus">
                                    <ct:Real>1</ct:Real>
                                    <Binop op="divide">
                                        <Binop op="times">
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="Imax"/>
                                            <ct:SymbRef symbIdRef="Cc"/>
                                        </Binop>
                                        <Binop op="plus">
                                            <ct:SymbRef symbIdRef="Cc"/>
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="IC50"/>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="kout"/>
                                <ct:SymbRef symbIdRef="E"/>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="t"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                                <Binop op="divide">
                                    <ct:SymbRef symbIdRef="Rin"/>
                                    <ct:SymbRef symbIdRef="kout"/>
                                </Binop>
                            </Equation>
                        </ct:Assign>
                    </ct:InitialValue>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            <ct:DerivativeVariable symbId="Cc" symbolType="real">
                <ct:Description>Concentration</ct:Description>
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="divide">
                            <ct:SymbRef symbIdRef="Ac"/>
                            <ct:SymbRef symbIdRef="V"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:DerivativeVariable>
        </StructuralModel>
        
        <!-- OBSERVATION MODEL -->
        <ObservationModel blkId="om1">
            <SimpleParameter symbId="a1"/>
            <SimpleParameter symbId="b1"/>
            <RandomVariable symbId="epsilon_Cc">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="obsErr" symbIdRef="residual"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Cc"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <Standard symbId="Cc_obs">
                <Output>
                    <ct:SymbRef blkIdRef="main" symbIdRef="Cc"></ct:SymbRef>
                </Output>
                <ErrorModel>
                    <ct:Assign>
                        <math:Equation>
                            <math:FunctionCall>
                                <ct:SymbRef symbIdRef="combinedErrorModel"/>
                                <math:FunctionArgument symbId="a">
                                    <ct:SymbRef symbIdRef="a1"/>
                                </math:FunctionArgument>
                                <math:FunctionArgument symbId="b">
                                    <ct:SymbRef symbIdRef="b1"/>
                                </math:FunctionArgument>
                                <math:FunctionArgument symbId="f">
                                    <math:Equation>
                                        <ct:SymbRef blkIdRef="main" symbIdRef="Cc"/>
                                    </math:Equation>
                                </math:FunctionArgument>
                            </math:FunctionCall>
                        </math:Equation>
                    </ct:Assign>
                </ErrorModel>
                <ResidualError>
                    <ct:SymbRef symbIdRef="epsilon_Cc"/>
                </ResidualError>
            </Standard>
        </ObservationModel>
        
        <ObservationModel blkId="om2">
            <SimpleParameter symbId="a2"/>
            <SimpleParameter symbId="g_C_E"/>
            <RandomVariable symbId="epsilon_E">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="obsErr" symbIdRef="residual"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_E"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <Correlation>
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="obsErr" symbIdRef="residual"/>
                </ct:VariabilityReference>
                <Pairwise>
                    <RandomVariable1>
                        <ct:SymbRef blkIdRef="om1" symbIdRef="epsilon_C"/>
                    </RandomVariable1>
                    <RandomVariable2>
                        <ct:SymbRef symbIdRef="epsilon_E"/>
                    </RandomVariable2>
                    <Covariance>
                        <ct:SymbRef symbIdRef="g_C_E"/>
                    </Covariance>
                </Pairwise>
            </Correlation>
            <General symbId="E_obs">
                <ct:Assign>
                    <math:Equation>
                        <math:Binop op="plus">
                            <ct:SymbRef blkIdRef="main" symbIdRef="E"/>
                            <math:Binop op="times">
                                <math:FunctionCall>
                                    <ct:SymbRef symbIdRef="constantErrorModel"/>
                                    <math:FunctionArgument symbId="a">
                                        <ct:SymbRef symbIdRef="a2"/>
                                    </math:FunctionArgument>
                                </math:FunctionCall>
                                <ct:SymbRef symbIdRef="epsilon_E"/>
                            </math:Binop>
                        </math:Binop>
                    </math:Equation>
                </ct:Assign>
            </General>
        </ObservationModel>
        
        <ObservationModel blkId="example">
            <SimpleParameter symbId="pop_a"/>
            <RandomVariable symbId="eta_a">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_a"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="a">
                <ct:Assign>
                    <math:Equation>
                        <math:Binop op="times">
                            <ct:SymbRef symbIdRef="pop_a"/>
                            <math:Uniop op="exp">
                                <ct:SymbRef symbIdRef="eta_a"/>
                            </math:Uniop>
                        </math:Binop>
                    </math:Equation>
                </ct:Assign>
            </IndividualParameter>
            <SimpleParameter symbId="pop_b"/>
            <RandomVariable symbId="eta_b">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_b"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="b">
                <ct:Assign>
                    <math:Equation>
                        <math:Binop op="times">
                            <ct:SymbRef symbIdRef="pop_b"/>
                            <math:Uniop op="exp">
                                <ct:SymbRef symbIdRef="eta_b"/>
                            </math:Uniop>
                        </math:Binop>
                    </math:Equation>
                </ct:Assign>
            </IndividualParameter>
            <SimpleParameter symbId="W">
                <ct:Assign>
                    <math:Equation>
                        <math:Binop op="root">
                            <math:Binop op="plus">
                                <math:Binop op="power">
                                    <ct:SymbRef symbIdRef="a"/>
                                    <ct:Int>2</ct:Int>
                                </math:Binop>
                                <math:Binop op="times">
                                    <math:Binop op="power">
                                        <ct:SymbRef symbIdRef="b"/>
                                        <ct:Int>2</ct:Int>
                                    </math:Binop>
                                    <math:Binop op="power">
                                        <ct:SymbRef blkIdRef="main" symbIdRef="f"/>
                                        <ct:Int>2</ct:Int>
                                    </math:Binop>
                                </math:Binop>
                            </math:Binop>
                            <ct:Int>2</ct:Int>
                        </math:Binop>
                    </math:Equation>
                </ct:Assign>
            </SimpleParameter>
            <RandomVariable symbId="epsilon_y">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="obsErr" symbIdRef="residual"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><prVal>1</prVal></stddev>
                </NormalDistribution>
            </RandomVariable>
            <Standard symbId="y">
                <Output>
                    <ct:SymbRef blkIdRef="main" symbIdRef="f"/>
                </Output>
                <ErrorModel>
                    <ct:Assign>
                        <ct:SymbRef symbIdRef="W"/>
                    </ct:Assign>
                </ErrorModel>
                <ResidualError>
                    <ct:SymbRef symbIdRef="epsilon_y"></ct:SymbRef>
                </ResidualError>
            </Standard>
        </ObservationModel>
    </ModelDefinition>
    
