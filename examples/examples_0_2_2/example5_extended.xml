<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/2013/03/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/2013/03/PharmML http://www.pharmml.org/2013/03/PharmML"
    xmlns:ct="http://www.pharmml.org/2013/03/CommonTypes"
    writtenVersion="0.2.2" id="i1">
    
    <ct:Name>Example 5 - estimation for growth tumor model (Ribba et al. 2012)</ct:Name>
    
    <ct:Description>based on A Tumor Growth Inhibition Model for Low-Grade Glioma Treated with Chemotherapy or Radiotherapy
        Benjamin Ribba, Gentian Kaloshi, Mathieu Peyre, et al. Clin Cancer Res Published OnlineFirst July 3, 2012.</ct:Description>
    
    <!-- intependent variable -->
    <IndependentVariable id="i2" symbId="time"/>

    <!-- SYMBOL DEFINITION - RESUIDUAL ERROR MODEL -->
    <ct:FunctionDefinition symbId="constantErrorModel" symbolType="real">
        <ct:FunctionArgument symbId="a" symbolType="real"/>
        <ct:Definition>
            <ct:SymbRef symbIdRef="a"/>
        </ct:Definition>
    </ct:FunctionDefinition>

    <!-- BLOCK I: MODEL DEFINITION -->
    <ModelDefinition id="i3"  xmlns="http://www.pharmml.org/2013/03/ModelDefinition">
        
        <!-- VARIABILITY MODEL -->
        <VariabilityModel id="i4" blkId="model" type="model"> 
            <Level symbId="indiv"/>
        </VariabilityModel>
        
        <VariabilityModel id="i5" blkId="obsErr" type="error"> 
            <Level symbId="residual"/>
        </VariabilityModel>
        
        <!-- PARAMETER MODEL -->
        <ParameterModel id="i6" blkId="pm1">
            <!-- **************************************** -->
            <!-- PARAMETERS: initial conditions           -->
            <!-- **************************************** -->
            <!-- C0 -->
            <SimpleParameter symbId="C0"/>
            <!-- P0 -->
            <SimpleParameter symbId="pop_P0"/>
            <SimpleParameter symbId="omega_P0"/>
            <RandomVariable symbId="eta_P0">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" 
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_P0"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="P0">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_P0"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_P0"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>            
            <!-- Q0 -->
            <SimpleParameter symbId="pop_Q0"/>
            <SimpleParameter symbId="omega_Q0"/>
            <RandomVariable symbId="eta_Q0">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Q0"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="Q0">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_Q0"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_Q0"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>            
            <!-- QP0 -->
            <SimpleParameter symbId="QP0">
                <ct:Assign>
                    <ct:Real>0</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            
            <!-- **************************************** -->
            <!-- PARAMETERS: population values & omegas   -->
            <!-- **************************************** -->
            <SimpleParameter symbId="K">
                <ct:Assign>
                    <ct:Real>100</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            <SimpleParameter symbId="pop_KDE"/>
            <SimpleParameter symbId="omega_KDE">
                <ct:Assign>
                    <ct:Real>0.5</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            <SimpleParameter symbId="pop_LAMBDAP"/>
            <SimpleParameter symbId="omega_LAMBDAP"/>
            <SimpleParameter symbId="pop_KQPP"/>
            <SimpleParameter symbId="omega_KQPP"/>
            <SimpleParameter symbId="pop_KPQ"/>
            <SimpleParameter symbId="omega_KPQ"/>
            <SimpleParameter symbId="pop_GAMA"/>
            <SimpleParameter symbId="omega_GAMA"/>
            <SimpleParameter symbId="pop_DELTAQP"/>
            <SimpleParameter symbId="omega_DELTAQP"/>
            
            <!-- LAMBDAP log-normal distributed -->
            <RandomVariable symbId="eta_LAMBDAP">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_LAMBDAP"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="LAMBDAP">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_LAMBDAP"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_LAMBDA"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- KQPP log-normal distributed -->
            <RandomVariable symbId="eta_KQPP">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_KQPP"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="KQPP">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_KQPP"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_KQPP"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- KPQ log-normal distributed -->
            <RandomVariable symbId="eta_KPQ">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_KPQ"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="KPQ">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_KPQ"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_KPQ"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- GAMMA log-normal distributed -->
            <RandomVariable symbId="eta_GAMMA">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_GAMMA"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="GAMMA">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_GAMMA"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_GAMMA"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- DELTAQP log-normal distributed -->
            <RandomVariable symbId="eta_DELTAQP">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_DELTAQP"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="DELTAQP">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_DELTAQP"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_DELTAQP"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
        </ParameterModel>

        <!-- STRUCTURAL MODEL 2 - ODEs -->
        <StructuralModel blkId="sm2">
            
            <!-- dC/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="C">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <Uniop op="minus">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                            </Uniop>
                            <ct:SymbRef symbIdRef="C"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="C0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                    <ct:InitialTime>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </ct:InitialTime>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
        </StructuralModel>
            
        <!-- STRUCTURAL MODEL 1 - ODEs -->
        <StructuralModel blkId="sm1">
            
            <!-- dC/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="C">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <Uniop op="minus">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                            </Uniop>
                            <ct:SymbRef symbIdRef="C"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>                            
                        </ct:Assign>
                    </ct:InitialValue>
                    <ct:InitialTime>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="C0"/>
                        </ct:Assign>
                    </ct:InitialTime>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dPT/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="PT">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="plus">
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="LAMBDAP"/>
                                <Binop op="times">
                                    <ct:SymbRef symbIdRef="PT"/>
                                    <Binop op="minus">
                                        <ct:Real>1</ct:Real>
                                        <Binop op="divide">
                                            <ct:SymbRef symbIdRef="PSTAR"/>
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="K"/>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                            <Binop op="minus">
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KQPP"/>
                                    <ct:SymbRef symbIdRef="QP"/>
                                </Binop>
                                <Binop op="minus">
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="KPQ"/>
                                        <ct:SymbRef symbIdRef="PT"/>
                                    </Binop>
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="GAMMA"/>
                                        <Binop op="times">
                                            <ct:SymbRef symbIdRef="C"/>
                                            <Binop op="times">
                                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                                <ct:SymbRef symbIdRef="PT"/>
                                            </Binop>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="P0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                    <ct:InitialTime>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>                           
                        </ct:Assign>
                    </ct:InitialTime>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dQ/dt --> 
            <ct:DerivativeVariable symbolType="real" symbId="Q">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="minus">
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="KPQ"/>
                            <Binop op="times">
                                <ct:SymbRef  blkIdRef="pm1" symbIdRef="GAMMA"/>
                                <Binop op="times">
                                    <ct:SymbRef symbIdRef="C"/>
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                        <ct:SymbRef symbIdRef="Q"/>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="Q0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                    <ct:InitialTime>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </ct:InitialTime>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dQP/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="QP">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="minus">
                            <Binop op="minus">
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="GAMMA"/>
                                    <Binop op="times">
                                        <ct:SymbRef symbIdRef="C"/>
                                        <Binop op="times">
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                            <ct:SymbRef symbIdRef="Q"/>
                                        </Binop>
                                    </Binop>
                                </Binop>
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KQPP"/>
                                    <ct:SymbRef symbIdRef="QP"/>
                                </Binop>
                            </Binop>
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="DELTAQP"/>
                                <ct:SymbRef symbIdRef="QP"/>
                            </Binop>                        
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="QP0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                    <ct:InitialTime>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>                            
                        </ct:Assign>
                    </ct:InitialTime>
                </ct:InitialCondition>
            </ct:DerivativeVariable>

            <!-- PSTAR -->
            <ct:Variable symbolType="real" symbId="PSTAR" >
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="plus">
                            <ct:SymbRef symbIdRef="PT"/>
                            <Binop op="plus">
                                <ct:SymbRef symbIdRef="Q"/>
                                <ct:SymbRef symbIdRef="QP"/>
                            </Binop>
                        </Binop>                    
                    </Equation>
                </ct:Assign>
            </ct:Variable>
        </StructuralModel>

        <!-- OBSERVATION MODEL - PSTAR -->
        <ObservationModel blkId="om1">
            <SimpleParameter symbId="a"/>
            <RandomVariable symbId="eps">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="obsErr" symbIdRef="residual"/>
                </ct:VariabilityReference>
                <!-- SLM: The definition is not the version, but the URL of the web definitions.
                    It should be definition="http://www.uncertml.org/distributions/normal"
                    -->
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <prVal>1</prVal>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <Standard symbId="PSTAR">
                <Output>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="PSTAR"/>
                </Output>
                <ErrorModel>
                    <ct:Assign>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <FunctionCall>
                                <ct:SymbRef symbIdRef="constantErrorModel"/>
                                <FunctionArgument symbId="a">
                                    <ct:SymbRef symbIdRef="a"/>
                                </FunctionArgument>
                            </FunctionCall>
                        </Equation>
                    </ct:Assign>
                </ErrorModel>
                <ResidualError>
                    <ct:SymbRef symbIdRef="eps"/>
                </ResidualError>
            </Standard>
        </ObservationModel>

    </ModelDefinition>
    
