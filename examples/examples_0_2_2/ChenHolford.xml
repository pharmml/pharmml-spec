                    <Row><ct:Id>i20</ct:Id><ct:Real>2</ct:Real><ct:Real>72</ct:Real></Row>
                    <Row><ct:Id>i20</ct:Id><ct:Real>74</ct:Real><ct:Real>4320</ct:Real></Row>
                    <Row><ct:Id>i20</ct:Id><ct:Real>4322</ct:Real><ct:Real>4392</ct:Real></Row>
                    <!-- ... -->
                    <Row><ct:Id>i20</ct:Id><ct:Real>17282</ct:Real><ct:Real>17352</ct:Real></Row>
                    <!-- ... -->
                </Table>
            </DataSet>
