<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/2013/03/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/2013/03/PharmML http://www.pharmml.org/2013/03/PharmML"

    xmlns:ct="http://www.pharmml.org/2013/03/CommonTypes"
    writtenVersion="0.2.1">
    
    <ct:Name>Example 6 - Chen, Holford model - model only</ct:Name>
    
    <IndependentVariable symbId="t"/>

    <!-- MODEL DEFINITION -->
    <ModelDefinition xmlns="http://www.pharmml.org/2013/03/ModelDefinition">
        
        <!-- VARIABILITY MODEL -->
        <VariabilityModel blkId="randomErrorModel" type="model">
            <Level symbId="subject"/>
            <Level symbId="trial">
                <ParentLevel>
                    <ct:SymbRef symbIdRef="subject"/>
                </ParentLevel>
            </Level>
            <Level symbId="occasion">
                <ParentLevel>
                    <ct:SymbRef symbIdRef="trial"/>
                </ParentLevel>
            </Level>
        </VariabilityModel>

        <VariabilityModel blkId="residualError" type="error">
            <Level symbId="residual"/>
        </VariabilityModel>
        
        <!-- COVARIATE MODEL -->
        <CovariateModel blkId="cm1">
            <!-- WEIGHT -->
            <Covariate symbId="W">
                <Continuous>
                    <Transformation>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <Binop op="divide">
                                <ct:SymbRef symbIdRef="W"/>
                                <ct:Real>70</ct:Real>
                            </Binop>                                
                        </Equation>
                    </Transformation>
                </Continuous>
            </Covariate>
        </CovariateModel>
        <!-- SEX -->
        <CovariateModel blkId="cm2">
            <Covariate symbId="SEX">
                <Categorical>
                    <Category catId="F"/>
                </Categorical>
            </Covariate>
        </CovariateModel>
        <!-- AGE -->
        <CovariateModel blkId="cm3">
            <Covariate symbId="AGE">
                <Continuous>
                    <Transformation>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <Binop op="minus">
                                <ct:SymbRef symbIdRef="AGE"/>
                                <ct:Real>60</ct:Real>
                            </Binop>
                        </Equation>
                    </Transformation>
                </Continuous>
            </Covariate>
        </CovariateModel>
        <!-- DD -->
        <CovariateModel blkId="cm4">
            <Covariate symbId="DD">
                <Categorical>
                    <Category catId="YES"/>
                </Categorical>
            </Covariate>
        </CovariateModel>
        
        <!-- PARAMETER MODEL -->
        <ParameterModel blkId="pm1">
            <!-- CL -->
            <SimpleParameter symbId="omega_Cl"/>
            <SimpleParameter symbId="pop_CL"/>
            <SimpleParameter symbId="beta_CL_W"/>
            <SimpleParameter symbId="beta_CL_SEX"/>
            <SimpleParameter symbId="beta_CL_AGE"/>
            <SimpleParameter symbId="beta_CL_DD"/>
            <!-- eta_CL_indiv -->
            <RandomVariable symbId="eta_CL_indiv">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="subject"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_CL"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- eta_CL_trial -->
            <RandomVariable symbId="eta_CL_trial">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="trial"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_CL"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- eta_CL_occasion -->
            <RandomVariable symbId="eta_CL_occasion">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="occasion"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_CL"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- CL -->
            <IndividualParameter symbId="CL">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <ct:SymbRef symbIdRef="pop_CL"/>
                            <Binop op="times">
                                <Binop op="power">
                                    <ct:SymbRef symbIdRef="W"/>
                                    <ct:SymbRef symbIdRef="beta_CL_W"/>
                                </Binop>
                                <Binop op="times">
                                    <Uniop op="exp">
                                        <Binop op="times">
                                            <ct:SymbRef symbIdRef="beta_CL_SEX"/>
                                            <ct:SymbRef blkIdRef="cm2" symbIdRef="SEX"/>
                                        </Binop>
                                    </Uniop>
                                    <Binop op="times">
                                        <Uniop op="exp">
                                            <Binop op="times">
                                                <ct:SymbRef symbIdRef="beta_CL_AGE"/>
                                                <ct:SymbRef blkIdRef="cm3" symbIdRef="AGE"/>
                                            </Binop>
                                        </Uniop>
                                        <Binop op="times">
                                            <Uniop op="exp">
                                                <Binop op="times">
                                                    <ct:SymbRef symbIdRef="beta_CL_DD"/>
                                                    <ct:SymbRef blkIdRef="cm4" symbIdRef="DD"/>
                                                </Binop>                                                
                                            </Uniop>
                                            <Binop op="times">
                                                <Uniop op="exp">
                                                    <ct:SymbRef symbIdRef="eta_indiv"/>
                                                </Uniop>
                                                <Binop op="times">
                                                    <Uniop op="exp">
                                                        <ct:SymbRef symbIdRef="eta_trial"/>
                                                    </Uniop>
                                                    <Uniop op="exp">
                                                        <ct:SymbRef symbIdRef="eta_occasion"/>
                                                    </Uniop>
                                                </Binop>
                                            </Binop>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>                   
                </ct:Assign>
            </IndividualParameter>
        </ParameterModel>
        
        <!-- STRUCTURAL MODEL -->
        <StructuralModel blkId="sm1">
            <!-- Ac -->
            <ct:DerivativeVariable symbolType="real" symbId="Ac">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">  
                        <Binop op="plus">
                            <Binop op="times">
                                <Binop op="divide">
                                    <ct:SymbRef symbIdRef="Rsyn"/>
                                    <ct:SymbRef symbIdRef="CL"/>
                                </Binop>
                                <ct:SymbRef symbIdRef="V1"/>
                            </Binop>
                            <Binop op="minus">
                                <Binop op="times">
                                    <Binop op="divide">
                                        <ct:SymbRef symbIdRef="Q"/>
                                        <ct:SymbRef symbIdRef="V2"/>
                                    </Binop>
                                    <ct:SymbRef symbIdRef="Ap"/>
                                </Binop>
                                <Binop op="minus">
                                    <Binop op="times">
                                        <Binop op="divide">
                                            <ct:SymbRef symbIdRef="Q"/>
                                            <ct:SymbRef symbIdRef="V1"/>
                                        </Binop>
                                        <ct:SymbRef symbIdRef="Ac"/>
                                    </Binop>
                                    <Binop op="times">
                                        <Binop op="divide">
                                            <ct:SymbRef symbIdRef="CL"/>
                                            <ct:SymbRef symbIdRef="V1"/>
                                        </Binop>
                                        <ct:SymbRef symbIdRef="Ac"/>
                                    </Binop>
                                </Binop>
                            </Binop>
                            
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:DerivativeVariable>
            <!-- Ap -->
            <ct:DerivativeVariable symbId="Ap" symbolType="real">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="minus">
                            <Binop op="times">
                                <Binop op="divide">
                                    <ct:SymbRef symbIdRef="Q"/>
                                    <ct:SymbRef symbIdRef="V1"/>
                                </Binop>
                                <ct:SymbRef symbIdRef="Ac"/>
                            </Binop>
                            <Binop op="times">
                                <Binop op="divide">
                                    <ct:SymbRef symbIdRef="Q"/>
                                    <ct:SymbRef symbIdRef="V2"/>
                                </Binop>
                                <ct:SymbRef symbIdRef="Ap"/>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:DerivativeVariable>
            <!-- C -->
            <ct:Variable symbolType="real" symbId="C">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="divide">
                            <ct:SymbRef symbIdRef="Ac"/>
                            <ct:SymbRef symbIdRef="V1"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:Variable>
        </StructuralModel>
        
        <!-- OBSERVATION MODEL -->
        <ObservationModel blkId="om1">
            <SimpleParameter symbId="a"/>
            <SimpleParameter symbId="b"/>
            <RandomVariable symbId="eta1">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="residualError"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><prVal>1</prVal></stddev>
                </NormalDistribution>
            </RandomVariable>
            <RandomVariable symbId="eta2">
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="residualError"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><prVal>1</prVal></stddev>
                </NormalDistribution>
            </RandomVariable>
            <General symbId="C_obs">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="plus">
                            <ct:SymbRef blkIdRef="sm1" symbIdRef="C"/>
                            <Binop op="plus">
                                <Binop op="times">
                                    <ct:SymbRef symbIdRef="a"/>
                                    <ct:SymbRef symbIdRef="eta1"/>
                                </Binop>
                                <Binop op="times">
                                    <ct:SymbRef symbIdRef="b"/>
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="sm1" symbIdRef="C"/>
                                        <ct:SymbRef symbIdRef="eta2"/>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </General>
        </ObservationModel>
        
    </ModelDefinition>
    
</PharmML>

