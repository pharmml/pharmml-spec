<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/2013/03/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/2013/03/PharmML http://www.pharmml.org/2013/03/PharmML"
    xmlns:ct="http://www.pharmml.org/2013/03/CommonTypes"
    writtenVersion="0.2.1">
    
    <!-- MODEL NAME -->
    <ct:Name>testDelete.xml</ct:Name>
    
    <ct:Description>based on Blub at al.</ct:Description>
    
    <!-- INDEPENDENT VARIABLE -->
    <IndependentVariable symbId="time"/>

    <!-- SYMBOL DEFINITION - RESUIDUAL ERROR MODEL -->
    <ct:FunctionDefinition symbId="constantErrorModel" symbolType="real">
        <ct:FunctionArgument symbId="a" symbolType="real"/>
        <ct:Definition>
            <ct:SymbRef symbIdRef="a"/>
        </ct:Definition>
    </ct:FunctionDefinition>

    <!-- BLOCK I: MODEL DEFINITION -->
    <ModelDefinition  xmlns="http://www.pharmml.org/2013/03/ModelDefinition">
        
        <!-- VARIABILITY MODEL -->
        <VariabilityModel blkId="modelVar" type="model"> 
            <Level symbId="indiv"/>
        </VariabilityModel>
        
        <VariabilityModel blkId="obsErr" type="error"> 
            <Level symbId="residual"/>
        </VariabilityModel>
        
        <CovariateModel blkId="cm1">
            <Covariate symbId="W">
                <Continuous>
                    <Transformation>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <Uniop op="log">
                                <Binop op="divide">
                                    <ct:SymbRef symbIdRef="W"/>
                                    <ct:Real>70</ct:Real>
                                </Binop>                                
                            </Uniop>
                        </Equation>
                    </Transformation>
                </Continuous>
            </Covariate>
        </CovariateModel>
        
        <!-- PARAMETER MODEL -->
        <ParameterModel blkId="pm1">
            <!-- **************************************** -->
            <!-- PARAMETERS: initial conditions           -->
            <!-- **************************************** -->
            <!-- C0 -->
            <SimpleParameter symbId="C0"/>
            <!-- P0 -->
            <SimpleParameter symbId="pop_P0"/>
            <SimpleParameter symbId="omega_P0"/>
            <RandomVariable symbId="eta_P0">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_P0"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="P0">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_P0"/>
                            </ct:Assign>
                        </PopulationParameter>
                        <Covariate>
                            <ct:SymbRef symbIdRef="W"/>
                            <FixedEffect>
                                <ct:SymbRef symbIdRef="beta_P0"/>
                            </FixedEffect>
                        </Covariate>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_P0"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>            
            <!-- Q0 -->
            <SimpleParameter symbId="pop_Q0"/>
            <SimpleParameter symbId="omega_Q0"/>
            <RandomVariable symbId="eta_Q0">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Q0"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="Q0">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_Q0"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_Q0"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>            
            <!-- QP0 -->
            <SimpleParameter symbId="QP0"/>
            
            
            <!-- **************************************** -->
            <!-- PARAMETERS: population values & omegas   -->
            <!-- **************************************** -->
            <SimpleParameter symbId="K"/>
            <SimpleParameter symbId="pop_KDE"/>
            <SimpleParameter symbId="omega_KDE"/>
            <SimpleParameter symbId="pop_LAMBDAP"/>
            <SimpleParameter symbId="omega_LAMBDAP"/>
            <SimpleParameter symbId="pop_KQPP"/>
            <SimpleParameter symbId="omega_KQPP"/>
            <SimpleParameter symbId="pop_KPQ"/>
            <SimpleParameter symbId="omega_KPQ"/>
            <SimpleParameter symbId="pop_GAMA"/>
            <SimpleParameter symbId="omega_GAMA"/>
            <SimpleParameter symbId="pop_DELTAQP"/>
            <SimpleParameter symbId="omega_DELTAQP"/>
            
            <!-- KDE  log-normal distributed -->
            <RandomVariable symbId="eta_KDE">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_KDE"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="KDE">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_KDE"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_KDE"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- LAMBDAP log-normal distributed -->
            <RandomVariable symbId="eta_LAMBDAP">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_LAMBDAP"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="LAMBDAP">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_LAMBDAP"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_LAMBDAP"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- KQPP log-normal distributed -->
            <RandomVariable symbId="eta_KQPP">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_KQPP"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="KQPP">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_KQPP"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_KQPP"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- KPQ log-normal distributed -->
            <RandomVariable symbId="eta_KPQ">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_KPQ"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="KPQ">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_KPQ"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_KPQ"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- GAMMA log-normal distributed -->
            <RandomVariable symbId="eta_GAMMA">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_GAMMA"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="GAMMA">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_GAMMA"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_GAMMA"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- DELTAQP log-normal distributed -->
            <RandomVariable symbId="eta_DELTAQP">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_DELTAQP"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="DELTAQP">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_DELTAQP"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_DELTAQP"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <Correlation>
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <RandomVariable1>
                    <ct:SymbRef symbIdRef="blub1"/>
                </RandomVariable1>
                <RandomVariable2>
                    <ct:SymbRef symbIdRef="blub2"></ct:SymbRef>
                </RandomVariable2>
                <CorrelationCoefficient>
                    <ct:Real>0.5</ct:Real>
                </CorrelationCoefficient>
            </Correlation>
        </ParameterModel>

        <!-- STRUCTURAL MODEL - ODEs -->
        <StructuralModel blkId="sm1">
            
            <ct:Variable symbolType="real" symbId="Q0">
                <ct:Assign>                         
                    <ct:Real>40</ct:Real>
                </ct:Assign>
            </ct:Variable>
            
            <!-- dC/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="C">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <Uniop op="minus">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                            </Uniop>
                            <ct:SymbRef symbIdRef="C"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:Assign>
                        <ct:SymbRef blkIdRef="pm1" symbIdRef="C0"/>
                    </ct:Assign>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dPT/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="PT">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="plus">
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="LAMBDAP"/>
                                <Binop op="times">
                                    <ct:SymbRef symbIdRef="PT"/>
                                    <Binop op="minus">
                                        <ct:Real>1</ct:Real>
                                        <Binop op="divide">
                                            <ct:SymbRef symbIdRef="PSTAR"/>
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="K"/>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                            <Binop op="minus">
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KQPP"/>
                                    <ct:SymbRef symbIdRef="QP"/>
                                </Binop>
                                <Binop op="minus">
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="KPQ"/>
                                        <ct:SymbRef symbIdRef="PT"/>
                                    </Binop>
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="GAMMA"/>
                                        <Binop op="times">
                                            <ct:SymbRef symbIdRef="C"/>
                                            <Binop op="times">
                                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                                <ct:SymbRef symbIdRef="PT"/>
                                            </Binop>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:Assign>
                        <ct:SymbRef blkIdRef="pm1" symbIdRef="P0"/>
                    </ct:Assign>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dQ/dt --> 
            <ct:DerivativeVariable symbolType="real" symbId="Q">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="minus">
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KPQ"/>
                                <ct:SymbRef symbIdRef="PT"/>
                            </Binop>
                            <Binop op="times">
                                <ct:SymbRef  blkIdRef="pm1" symbIdRef="GAMMA"/>
                                <Binop op="times">
                                    <ct:SymbRef symbIdRef="C"/>
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                        <ct:SymbRef symbIdRef="Q"/>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:Assign>
                        <ct:SymbRef blkIdRef="pm1" symbIdRef="Q0"/>
                    </ct:Assign>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dQP/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="QP">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="minus">
                            <Binop op="minus">
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="GAMMA"/>
                                    <Binop op="times">
                                        <ct:SymbRef symbIdRef="C"/>
                                        <Binop op="times">
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                            <ct:SymbRef symbIdRef="Q"/>
                                        </Binop>
                                    </Binop>
                                </Binop>
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KQPP"/>
                                    <ct:SymbRef symbIdRef="QP"/>
                                </Binop>
                            </Binop>
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="DELTAQP"/>
                                <ct:SymbRef symbIdRef="QP"/>
                            </Binop>                        
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:Assign>
                        <ct:SymbRef blkIdRef="pm1" symbIdRef="QP0"/>
                    </ct:Assign>
                </ct:InitialCondition>
            </ct:DerivativeVariable>

            <!-- PSTAR -->
            <ct:Variable symbolType="real" symbId="PSTAR" >
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="plus">
                            <ct:SymbRef symbIdRef="PT"/>
                            <Binop op="plus">
                                <ct:SymbRef symbIdRef="Q"/>
                                <ct:SymbRef symbIdRef="QP"/>
                            </Binop>
                        </Binop>                    
                    </Equation>
                </ct:Assign>
            </ct:Variable>
        </StructuralModel>

    </ModelDefinition>
 
</PharmML>
