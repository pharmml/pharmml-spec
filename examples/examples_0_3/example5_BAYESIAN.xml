<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/2013/03/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/2013/03/PharmML http://www.pharmml.org/2013/03/PharmML"
    xmlns:ct="http://www.pharmml.org/2013/03/CommonTypes"
    writtenVersion="0.3" id="i1">
    
    <!-- MODEL NAME -->
    <ct:Name>Example 5 - estimation for growth tumor model</ct:Name>
    
    <ct:Description>based on A Tumor Growth Inhibition Model for Low-Grade Glioma Treated with Chemotherapy or Radiotherapy
        Benjamin Ribba, Gentian Kaloshi, Mathieu Peyre, et al. Clin Cancer Res Published OnlineFirst July 3, 2012.</ct:Description>
    
    <!-- INDEPENDENT VARIABLE -->
    <IndependentVariable symbId="time"/>

    <!-- SYMBOL DEFINITION - RESUIDUAL ERROR MODEL -->
    <ct:FunctionDefinition symbId="constantErrorModel" symbolType="real">
        <ct:FunctionArgument symbId="a" symbolType="real"/>
        <ct:Definition>
            <ct:SymbRef symbIdRef="a"/>
        </ct:Definition>
    </ct:FunctionDefinition>

    <!-- BLOCK I: MODEL DEFINITION -->
    <ModelDefinition  xmlns="http://www.pharmml.org/2013/03/ModelDefinition">
        
        <!-- VARIABILITY MODEL -->
        <VariabilityModel blkId="modelVar" type="parameterVariability"> 
            <Level symbId="indiv"/>
        </VariabilityModel>
        
        <VariabilityModel blkId="obsErr" type="residualError"> 
            <Level symbId="residual"/>
        </VariabilityModel>
        
        <!-- PARAMETER MODEL -->
        <ParameterModel blkId="pm1">
            <!-- **************************************** -->
            <!-- PARAMETERS: initial conditions           -->
            <!-- **************************************** -->
            <!-- C0 -->
            <SimpleParameter symbId="C0"/>
            <!-- P0 -->
            <SimpleParameter symbId="pop_P0"/>
            <SimpleParameter symbId="omega_P0"/>
            <RandomVariable symbId="eta_P0">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_P0"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="P0">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_P0"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_P0"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>            
            <!-- Q0 -->
            <SimpleParameter symbId="pop_Q0"/>
            <SimpleParameter symbId="omega_Q0"/>
            <RandomVariable symbId="eta_Q0">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Q0"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="Q0">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_Q0"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_Q0"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>            
            <!-- QP0 -->
            <SimpleParameter symbId="QP0">
                <ct:Assign>
                    <ct:Real>0</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            
            <!-- **************************************** -->
            <!-- PARAMETERS: population values & omegas   -->
            <!-- **************************************** -->
            <SimpleParameter symbId="K">
                <ct:Assign>
                    <ct:Real>100</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            <SimpleParameter symbId="pop_KDE"/>
            <SimpleParameter symbId="omega_KDE">
                <ct:Assign>
                    <ct:Real>0.5</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            <SimpleParameter symbId="pop_LAMBDAP"/>
            <SimpleParameter symbId="omega_LAMBDAP"/>
            <SimpleParameter symbId="pop_KQPP"/>
            <SimpleParameter symbId="omega_KQPP"/>
            <SimpleParameter symbId="pop_KPQ"/>
            <SimpleParameter symbId="omega_KPQ"/>
            <SimpleParameter symbId="pop_GAMA"/>
            <SimpleParameter symbId="omega_GAMA"/>
            <SimpleParameter symbId="pop_DELTAQP"/>
            <SimpleParameter symbId="omega_DELTAQP"/>
            
            <!-- LAMBDAP log-normal distributed -->
            <RandomVariable symbId="eta_LAMBDAP">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_LAMBDAP"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="LAMBDAP">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_LAMBDAP"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_LAMBDA"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- KQPP log-normal distributed -->
            <RandomVariable symbId="eta_KQPP">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_KQPP"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="KQPP">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_KQPP"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_KQPP"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- KPQ log-normal distributed -->
            <RandomVariable symbId="eta_KPQ">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_KPQ"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="KPQ">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_KPQ"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_KPQ"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- GAMMA log-normal distributed -->
            <RandomVariable symbId="eta_GAMMA">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_GAMMA"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="GAMMA">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_GAMMA"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_GAMMA"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- DELTAQP log-normal distributed -->
            <RandomVariable symbId="eta_DELTAQP">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_DELTAQP"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="DELTAQP">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_DELTAQP"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_DELTAQP"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
        </ParameterModel>

        <!-- STRUCTURAL MODEL - ODEs -->
        <StructuralModel blkId="sm1">
            
            <!-- dC/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="C">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <Uniop op="minus">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                            </Uniop>
                            <ct:SymbRef symbIdRef="C"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="C0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                    <ct:InitialTime>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </ct:InitialTime>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dPT/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="PT">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="plus">
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="LAMBDAP"/>
                                <Binop op="times">
                                    <ct:SymbRef symbIdRef="PT"/>
                                    <Binop op="minus">
                                        <ct:Real>1</ct:Real>
                                        <Binop op="divide">
                                            <ct:SymbRef symbIdRef="PSTAR"/>
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="K"/>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                            <Binop op="minus">
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KQPP"/>
                                    <ct:SymbRef symbIdRef="QP"/>
                                </Binop>
                                <Binop op="minus">
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="KPQ"/>
                                        <ct:SymbRef symbIdRef="PT"/>
                                    </Binop>
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="GAMMA"/>
                                        <Binop op="times">
                                            <ct:SymbRef symbIdRef="C"/>
                                            <Binop op="times">
                                                <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                                <ct:SymbRef symbIdRef="PT"/>
                                            </Binop>
                                        </Binop>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="P0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                    <ct:InitialTime>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </ct:InitialTime>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dQ/dt --> 
            <ct:DerivativeVariable symbolType="real" symbId="Q">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="minus">
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="KPQ"/>
                            <Binop op="times">
                                <ct:SymbRef  blkIdRef="pm1" symbIdRef="GAMMA"/>
                                <Binop op="times">
                                    <ct:SymbRef symbIdRef="C"/>
                                    <Binop op="times">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                        <ct:SymbRef symbIdRef="Q"/>
                                    </Binop>
                                </Binop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="Q0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                    <ct:InitialTime>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </ct:InitialTime>
                </ct:InitialCondition>
            </ct:DerivativeVariable>
            
            <!-- dQP/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="QP">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="minus">
                            <Binop op="minus">
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="GAMMA"/>
                                    <Binop op="times">
                                        <ct:SymbRef symbIdRef="C"/>
                                        <Binop op="times">
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="KDE"/>
                                            <ct:SymbRef symbIdRef="Q"/>
                                        </Binop>
                                    </Binop>
                                </Binop>
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="KQPP"/>
                                    <ct:SymbRef symbIdRef="QP"/>
                                </Binop>
                            </Binop>
                            <Binop op="times">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="DELTAQP"/>
                                <ct:SymbRef symbIdRef="QP"/>
                            </Binop>                        
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="time"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:InitialValue>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="QP0"/>
                        </ct:Assign>
                    </ct:InitialValue>
                    <ct:InitialTime>
                        <ct:Assign>
                            <ct:Real>0</ct:Real>
                        </ct:Assign>
                    </ct:InitialTime>
                </ct:InitialCondition>
            </ct:DerivativeVariable>

            <!-- PSTAR -->
            <ct:Variable symbolType="real" symbId="PSTAR" >
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="plus">
                            <ct:SymbRef symbIdRef="PT"/>
                            <Binop op="plus">
                                <ct:SymbRef symbIdRef="Q"/>
                                <ct:SymbRef symbIdRef="QP"/>
                            </Binop>
                        </Binop>                    
                    </Equation>
                </ct:Assign>
            </ct:Variable>
        </StructuralModel>

        <!-- OBSERVATION MODEL - PSTAR -->
        <ObservationModel blkId="om1">
            <SimpleParameter symbId="a"/>
            <RandomVariable symbId="eps">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="obsErr" symbIdRef="residual"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <prVal>1</prVal>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <Standard symbId="PSTAR_obs">
                <Output>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="PSTAR"/>
                </Output>
                <ErrorModel>
                    <ct:Assign>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <FunctionCall>
                                <ct:SymbRef symbIdRef="constantErrorModel"/>
                                <FunctionArgument symbId="a">
                                    <ct:SymbRef symbIdRef="a"/>
                                </FunctionArgument>
                            </FunctionCall>
                        </Equation>
                    </ct:Assign>
                </ErrorModel>
                <ResidualError>
                    <ct:SymbRef symbIdRef="eps"/>
                </ResidualError>
            </Standard>
        </ObservationModel>

    </ModelDefinition>
    
    <!-- BLOCK II: TRIAL DEFINITION -->
    <TrialDesign xmlns="http://www.pharmml.org/2013/03/TrialDesign">
        
        <!-- STRUCTURE -->
        <Structure>
            <Epoch oid="epoch1">
                <Start>
                    <ct:Real>0</ct:Real>
                </Start>
                <End>
                    <ct:Real>200</ct:Real>
                </End>
                <Order>1</Order>
            </Epoch>
            <Arm oid="arm1"/>
            <Cell oid="cell1">
                <EpochRef oidRef="epoch1"/>
                <ArmRef oidRef="arm1"/>
                <SegmentRef oidRef="TA"/>
            </Cell>
            <Segment oid="TA">
                <ActivityRef oidRef="bolusIV"/>
            </Segment>
            <Activity oid="bolusIV">
                <Bolus>
                    <DoseAmount inputTarget="derivativeVariable">
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="C"/>
                    </DoseAmount>
                </Bolus>
            </Activity>
        </Structure> 
        
        <!-- POPULATION -->
        <Population> 
            <ct:VariabilityReference>
                <ct:SymbRef blkIdRef="modelVar" symbIdRef="indiv"/>
            </ct:VariabilityReference>
            
            <DataSet xmlns="http://www.pharmml.org/2013/08/Dataset">
                <Definition>
                    <Column columnId="ID" columnType="id" valueType="id" columnNum="1"/> 
                    <Column columnId="ARM" columnType="arm" valueType="id" columnNum="2"/> 
                    <Column columnId="REP" columnType="replicate" valueType="int" columnNum="3"/> 
                </Definition>
                <Table>
                    <Row>
                        <ct:Id>i</ct:Id>
                        <ct:Id>arm1</ct:Id>
                        <ct:Int>21</ct:Int>
                    </Row>
                </Table>
            </DataSet>
        </Population>

        <!-- INDIVUIDUAL DOSING -->
        <IndividualDosing>
            
            <ActivityRef oidRef="bolusIV"/>
            
            <ColumnMapping>
                <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="TIME"/>
                <ct:SymbRef symbIdRef="time"/>
            </ColumnMapping>
            
            <DataSet xmlns="http://www.pharmml.org/2013/08/Dataset">
                <Definition>
                    <Column columnId="ID" columnType="id" valueType="id" columnNum="1"/>
                    <Column columnId="TIME" columnType="time" valueType="real" columnNum="2"/>
                    <Column columnId="DOSE" columnType="dose" valueType="real" columnNum="3"/>
                </Definition>
                <ImportData oid="PCV_dosingData">
                    <name>RIBBA2012_PCV_dosing</name>
                    <url>file:///examples/examples_0_2_2/</url>
                    <format>CSV</format>
                    <delimiter>COMMA</delimiter>
                </ImportData>
            </DataSet>
        </IndividualDosing>
    </TrialDesign>
    
    <!-- BLOCK III: MODELLING STEPS -->
    <ModellingSteps xmlns="http://www.pharmml.org/2013/03/ModellingSteps">
        
        <!-- ESTIMATION STEP -->
        <EstimationStep oid="estTask1">

            <ObjectiveDataSet>
                
                <ColumnMapping>
                    <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="TIME"/> 
                    <ct:SymbRef symbIdRef="time"/>
                </ColumnMapping>
                
                <ColumnMapping>
                    <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="DV"/> 
                    <ct:SymbRef blkIdRef="om1" symbIdRef="PSTAR_obs"/>
                </ColumnMapping>
                
                <DataSet xmlns="http://www.pharmml.org/2013/08/Dataset">
                    <Definition>
                        <Column columnId="ID" columnType="id" valueType="id" columnNum="1"/>
                        <Column columnId="TIME" columnType="idv" valueType="real" columnNum="2"/>
                        <Column columnId="DV" columnType="dv" valueType="real" columnNum="3"/>
                    </Definition>
                    <ImportData oid="PCV_observationData">
                        <name>RIBBA2012_PCV_observations</name>
                        <url>file:///examples/examples_0_2_2/</url>
                        <format>CSV</format>
                        <delimiter>COMMA</delimiter>
                    </ImportData>                    
                </DataSet>
            </ObjectiveDataSet>
            
            <ParametersToEstimate>
                <!-- INITIAL CONDITIONS -->
                <!-- P0 -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_P0"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_P0"/>
                    <InitialEstimate>
                        <ct:Real>0.5</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <!-- Q0 -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_Q0"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_Q0"/>
                    <InitialEstimate>
                        <ct:Real>0.5</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
                <!-- RESIDUAL ERROR PARAMETER -->
                <!-- a -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="om1" symbIdRef="a"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
                <!-- STRUCTURAL PARAMETERS and RANDOM EFFECTS -->
                <!-- LAMBDAP -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_LAMBDAP"/>
                    <InitialEstimate>
                        <ct:Real>10</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_LAMBDAP"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
                <!-- KDE -->
                <!-- omega_KDE is fixed, see pm1 -->
                <!-- only pop_KDE to be estimated -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_KDE"/>
                    <InitialEstimate>
                        <ct:Real>10</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>

                <!-- KQPP -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_KQPP"/>
                    <InitialEstimate>
                        <ct:Real>10</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_KQPP"/>
                    <InitialEstimate>
                        <ct:Real>1</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                
                <!-- LAMBDAP -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_LAMBDAP"/>
                    <InitialEstimate>
                        <ct:Real>10</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
