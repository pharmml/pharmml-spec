<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/2013/03/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/2013/03/PharmML http://www.pharmml.org/2013/03/PharmML"
    xmlns:ct="http://www.pharmml.org/2013/03/CommonTypes"
    writtenVersion="0.2.1">
    
    <ct:Name id="i1">Tham-JCRR-2008</ct:Name>
    
    <ct:Description id="i2">This model is based on the publication:A pharmacodynamic model for the time course of tumour shrinkage by gemcitabine + carboplatin in non-smallcell lung cancer patients
        Lai-San Tham, Lingzhi Wang, Ross A. Soo, et al. Clin Cancer Res 2008; 14(13), 2008.</ct:Description>
    
    
    <!-- independent variable -->
    <IndependentVariable symbId="t"/>

    <!-- SYMBOL DEFINITION - RESIDUAL ERROR MODEL -->
    <FunctionDefinition xmlns="http://www.pharmml.org/2013/03/CommonTypes"
        symbId="combinedErrorModel" symbolType="real">
        <FunctionArgument symbId="a" symbolType="real"/>
        <FunctionArgument symbId="b" symbolType="real"/>
        <FunctionArgument symbId="f" symbolType="real"/>
        <Definition>
            <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                <Binop op="plus">
                    <ct:SymbRef symbIdRef="a"/>
                    <Binop op="times">
                        <ct:SymbRef symbIdRef="b"/>
                        <ct:SymbRef symbIdRef="f"/>
                    </Binop>
                </Binop>
            </Equation>
        </Definition>
    </FunctionDefinition>

    <!-- BLOCK I: MODEL DEFINITION -->
    <ModelDefinition  xmlns="http://www.pharmml.org/2013/03/ModelDefinition">
        
        <!-- BLOCK I part A: VARIABILITY MODEL -->
        <VariabilityModel blkId="model" type="model"> 
            <Level symbId="indiv"/>
        </VariabilityModel>        
        <VariabilityModel blkId="obsErr" type="error"> 
            <Level symbId="residual"/>
        </VariabilityModel>
 
        <!-- BLOCK I part B: COVARIATE MODEL -->
        
        <!-- BLOCK I part C: PARAMETER MODEL -->
        
        <ParameterModel blkId="pm1">
            
            <!-- ***************************************************************** -->
            <!-- PARAMETERS: at initial conditions withh IIV: only SIZE0           -->
            <!-- ***************************************************************** -->
            <!-- SIZE0 -->
            <SimpleParameter symbId="pop_SIZE0"/>
            <SimpleParameter symbId="omega_SIZE0"/>
            <RandomVariable symbId="eta_SIZE0">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" 
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_SIZE0"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="SIZE0" id="i13">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_SIZE0"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_SIZE0"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- ******************************************** -->
            <!-- PARAMETERS: population values & omegas       -->
            <!-- WH, MGMM, TOVER, AE50, TEQ                   -->
            <!-- TOVER=TOVER*WH change unit from Week to Hour -->
            <!-- AE50=AE50/MGMM change unit from mg to Âµmole  -->
            <!-- TEQ=TEQ*WH change unit from Week to Hour     -->
            <!-- ******************************************** -->
            <!-- derived parameters:                          -->
            <!-- KOVER=1/TOVER                                -->
            <!-- RIN=SIZE0*KOVER                              -->
            <!-- KPD=LOG(2)/TEQ                               -->
            <!-- PD=1-A2/(A2+AE50)                            -->            
            <!-- ******************************************** -->
            <SimpleParameter symbId="WH">
                <ct:Assign>
                    <ct:Real>168</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            <SimpleParameter symbId="MGMM">
                <ct:Assign>
                    <ct:Real>263000</ct:Real>
                </ct:Assign>
            </SimpleParameter>
            <SimpleParameter symbId="TEQ"/>
            
            <!-- TOVER log-normal distributed -->
            <SimpleParameter symbId="pop_TOVER"/>
            <SimpleParameter symbId="omega_TOVER"/>
            <RandomVariable symbId="eta_TOVER">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_TOVER"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="TOVER">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_TOVER"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_TOVER"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
            
            <!-- AE50 log-normal distributed -->
            <SimpleParameter symbId="pop_AE50"/>
            <SimpleParameter symbId="omega_AE50"/>
            <RandomVariable symbId="eta_AE50">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="http://www.uncertml.org/distributions/normal">
                    <mean>
                        <rVal>0</rVal>
                    </mean>
                    <stddev>
                        <var varId="omega_AE50"/>
                    </stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="AE50">
                <GaussianModel>
                    <Transformation>log</Transformation>
                    <LinearCovariate>
                        <PopulationParameter>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="pop_AE50"/>
                            </ct:Assign>
                        </PopulationParameter>
                    </LinearCovariate>
                    <RandomEffects>
                        <ct:SymbRef symbIdRef="eta_AE50"/>
                    </RandomEffects>
                </GaussianModel>
            </IndividualParameter>
                        
        </ParameterModel>

        <!-- BLOCK I part D: STRUCTURAL MODEL -->
        
        <StructuralModel blkId="sm1" id="i6">
            
            <!-- dA1/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="A1" id="i5">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <Binop op="divide">
                                <Uniop op="minus">
                                    <Uniop op="log">
                                        <ct:Real>
                                            2
                                        </ct:Real>
                                    </Uniop>
                                </Uniop>
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="TEQ"/>                               
                            </Binop>
                            <ct:SymbRef symbIdRef="A1"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="t"/>
                </ct:IndependentVariable>
            </ct:DerivativeVariable>
            
            <!-- dA2/dt -->
            <ct:DerivativeVariable symbolType="real" symbId="A2" id="i16">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <Binop op="minus">
                                <Binop op="times">
                                    <Binop op="divide">
                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="SIZE0"/>
                                        <Binop op="times">
                                            <ct:SymbRef blkIdRef="pm1" symbIdRef="TOVER"/>
                                            <ct:Real>168</ct:Real>
                                        </Binop>
                                    </Binop>
                                    <Binop op="minus">
                                        <ct:Real>1</ct:Real>
                                        <Binop op="divide">
                                            <ct:SymbRef symbIdRef="A1"/>
                                            <Binop op="plus">
                                                <ct:SymbRef symbIdRef="A1"/>
                                                <Binop op="divide">
                                                    <Binop op="times">
                                                        <ct:SymbRef blkIdRef="pm1" symbIdRef="AE50"/>
                                                        <ct:Real>1000</ct:Real>
                                                    </Binop>
                                                    <ct:Real>263</ct:Real>
                                                </Binop>
                                            </Binop>                                           
                                        </Binop>                                        
                                    </Binop>
                                </Binop>
                                <Binop op="divide">
                                    <ct:SymbRef symbIdRef="A2"/>
                                    <Binop op="times">
                                        <ct:SymbRef symbIdRef="TOVER"/>
                                        <ct:Real>168</ct:Real>
                                    </Binop>
                                </Binop>
                            </Binop>
                            <ct:SymbRef symbIdRef="A2"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
                <ct:IndependentVariable>
                    <ct:SymbRef symbIdRef="t"/>
                </ct:IndependentVariable>
                <ct:InitialCondition>
                    <ct:Assign>
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="SIZE0"/>
                    </ct:Assign>
                </ct:InitialCondition>
            </ct:DerivativeVariable>                        
        </StructuralModel>

        <!-- BLOCK I part E: OBSERVATION MODEL -->
                        
        <ObservationModel blkId="om1">
            <SimpleParameter symbId="a"/>
            <SimpleParameter symbId="b"/>
            <RandomVariable symbId="epsilon_A2">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="obsErr" symbIdRef="residual"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0"
                    definition="http://www.uncertml.org/distributions/normal">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="sigma_A2"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <Standard symbId="A2_obs">
                <Output>
                    <ct:SymbRef blkIdRef="sm1" symbIdRef="A2"/>
                </Output>
                <ErrorModel>
                    <ct:Assign>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <FunctionCall>
                                <ct:SymbRef symbIdRef="combinedErrorModel"/>
                                <FunctionArgument symbId="a">
                                    <ct:SymbRef symbIdRef="a"/>
                                </FunctionArgument>
                                <FunctionArgument symbId="b">
                                    <ct:SymbRef symbIdRef="b"/>
                                </FunctionArgument>
                                <FunctionArgument symbId="f">
                                    <Equation>
                                        <ct:SymbRef blkIdRef="sm1" symbIdRef="A2"/>
                                    </Equation>
                                </FunctionArgument>
                            </FunctionCall>
                        </Equation>
                    </ct:Assign>
                </ErrorModel>
                <ResidualError>
                    <ct:SymbRef symbIdRef="epsilon_A2"/>
                </ResidualError>
            </Standard>
        </ObservationModel>
    </ModelDefinition>    
            
    <!-- BLOCK II: TRIAL DEFINITION -->
    <TrialDesign xmlns="http://www.pharmml.org/2013/03/TrialDesign">
        
        <!-- STRUCTURE -->
        <Structure id="i7">
            <Epoch oid="epoch1">
                <Start>
                    <ct:Real>0</ct:Real>
                </Start>
                <End>
                    <ct:Real>17880</ct:Real>
                </End>
                <Order>1</Order>
            </Epoch>
            <Arm oid="arm1"/>
            <Cell oid="cell1">
                <EpochRef oidRef="epoch1"/>
                <ArmRef oidRef="arm1"/>
                <SegmentRef oidRef="TA"/>
            </Cell>
            <Segment oid="TA">
                <ActivityRef oidRef="bolusIV"/>
            </Segment>
            <Activity oid="bolusIV">
                <Bolus>
                    <DoseAmount inputType="target">
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="A1"/>
                    </DoseAmount>
                </Bolus>
            </Activity>
        </Structure> 
        
        <!-- POPULATION -->
        <Population> 
            <IndividualTemplate> 
                <IndividualMapping> 
                    <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="ID"/> 
                </IndividualMapping> 
                <ArmMapping> 
                    <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="ARM"/> 
                </ArmMapping> 
                <ReplicateMapping> 
                    <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="REP"/> 
                </ReplicateMapping>
            </IndividualTemplate>
            <DataSet xmlns="http://www.pharmml.org/2013/08/Dataset">
                <Definition>
                    <Column columnId="ID" valueType="id" columnNum="1"/> 
                    <Column columnId="ARM" valueType="id" columnNum="2"/> 
                    <Column columnId="REP" valueType="int" columnNum="3"/> 
                </Definition>
                <Table>
                    <Row>
                        <ct:Id>i</ct:Id>
                        <ct:Id>arm1</ct:Id>
                        <ct:Int>21</ct:Int>
                    </Row>
                </Table>
            </DataSet>
        </Population>

        <!-- INDIVUIDUAL DOSING -->
        <IndividualDosing>
            <ActivityRef oidRef="bolusIV"/>
            <IndividualRef columnIdRef="ID"/>
            <DataSet xmlns="http://www.pharmml.org/2013/08/Dataset">
                <Definition>
                    <Column columnId="ID" valueType="id" columnNum="1"/>
                    <Column columnId="TIME" valueType="real" columnNum="2"/>
                    <Column columnId="AMT" valueType="real" columnNum="3" id="i12"/>
                </Definition>
                <Table>
                    <!-- subject 1 -->
                    <Row><ct:Id>i1</ct:Id><ct:Real>0</ct:Real><ct:Real>5448.7</ct:Real></Row> 
                    <Row><ct:Id>i1</ct:Id><ct:Real>168</ct:Real><ct:Real>5448.7</ct:Real></Row> 
                    <Row><ct:Id>i1</ct:Id><ct:Real>840</ct:Real><ct:Real>5448.7</ct:Real></Row> 
                    <Row><ct:Id>i1</ct:Id><ct:Real>1176</ct:Real><ct:Real>5448.7</ct:Real></Row> 
                    <Row><ct:Id>i1</ct:Id><ct:Real>1344</ct:Real><ct:Real>5448.7</ct:Real></Row> 
                    <Row><ct:Id>i1</ct:Id><ct:Real>1848</ct:Real><ct:Real>4186.3</ct:Real></Row> 
                    <Row><ct:Id>i1</ct:Id><ct:Real>2016</ct:Real><ct:Real>2794.7</ct:Real></Row> 
                    <!-- subject 2 -->
                    <Row><ct:Id>i2</ct:Id><ct:Real>0</ct:Real><ct:Real>5589.4</ct:Real></Row> 
                    <Row><ct:Id>i2</ct:Id><ct:Real>168</ct:Real><ct:Real>4190.1</ct:Real></Row> 
                    <Row><ct:Id>i2</ct:Id><ct:Real>528</ct:Real><ct:Real>4308</ct:Real></Row> 
                    <Row><ct:Id>i2</ct:Id><ct:Real>840</ct:Real><ct:Real>4308</ct:Real></Row>  
                    <!-- SNIP -->
                    <!-- subject 56 -->
                    <Row><ct:Id>i56</ct:Id><ct:Real>0</ct:Real><ct:Real>6349.8</ct:Real></Row> 
                    <Row><ct:Id>i56</ct:Id><ct:Real>504</ct:Real><ct:Real>6273.8</ct:Real></Row> 
                    <Row><ct:Id>i56</ct:Id><ct:Real>672</ct:Real><ct:Real>6273.8</ct:Real></Row> 
                    <Row><ct:Id>i56</ct:Id><ct:Real>1224</ct:Real><ct:Real>6425.9</ct:Real></Row> 
                    <Row><ct:Id>i56</ct:Id><ct:Real>1560</ct:Real><ct:Real>6463.9</ct:Real></Row> 
                    <Row><ct:Id>i56</ct:Id><ct:Real>1728</ct:Real><ct:Real>6463.9</ct:Real></Row> 
                    <Row><ct:Id>i56</ct:Id><ct:Real>2232</ct:Real><ct:Real>6501.9</ct:Real></Row> 
                    <Row><ct:Id>i56</ct:Id><ct:Real>2736</ct:Real><ct:Real>6616</ct:Real></Row> 
                    <Row><ct:Id>i56</ct:Id><ct:Real>2904</ct:Real><ct:Real>6616</ct:Real></Row> 
                </Table>
            </DataSet>
        </IndividualDosing>
    </TrialDesign>
    
    <!-- BLOCK III: MODELLING STEPS -->
    <ModellingSteps xmlns="http://www.pharmml.org/2013/03/ModellingSteps">
        
        <!-- ESTIMATION STEP -->
        <EstimationStep oid="estTask1" id="i10">
            <ObjectiveDataSet id="i8">
                <IndividualMapping>
                    <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="ID"/>
                </IndividualMapping>
                <VariableMapping>
                    <ColumnRef xmlns="http://www.pharmml.org/2013/08/Dataset" columnIdRef="TIME"/> 
