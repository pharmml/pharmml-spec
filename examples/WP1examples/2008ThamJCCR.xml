                    </Table>

                </DataSet>
            </ObjectiveDataSet>
            <ParametersToEstimate>
                <!-- INITIAL CONDITIONS -->
                <!-- SIZE0 -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_SIZE0"/>
                    <InitialEstimate>
                        <ct:Real>6.66</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="omega_SIZE0"/>
                    <InitialEstimate>
                        <ct:Real>0.317</ct:Real>
                    </InitialEstimate>
                </ParameterEstimation>
                <!-- TOVER -->
                <ParameterEstimation>
                    <ct:SymbRef blkIdRef="pm1" symbIdRef="pop_TOVER"/>
