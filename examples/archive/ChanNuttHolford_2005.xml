<?xml version="1.0" encoding="UTF-8"?>
<PharmML xmlns="http://www.pharmml.org/2013/03/PharmML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pharmml.org/2013/03/PharmML http://www.pharmml.org/2013/03/PharmML"
    xmlns:ct="http://www.pharmml.org/2013/03/CommonTypes"
    xmlns:ds="http://www.pharmml.org/2013/08/Dataset"
    writtenVersion="0.1">
    <ct:Name>Levodopa Pharmacokinetic Variability</ct:Name>
    <ct:Description>Chan, Nutt, Holford 2005 Parkinson paper</ct:Description>
    <IndependentVariable symbId="t"/>
    <!-- BLOCK I: MODEL DEFINITION -->
    <ModelDefinition xmlns="http://www.pharmml.org/2013/03/ModelDefinition">
        <VariabilityModel blkId="model" type="model">
            <Level symbId="indiv"/>
            <Level symbId="trial">
                <ParentLevel>
                    <ct:SymbRef symbIdRef="indiv"/>
                </ParentLevel>
            </Level>
            <Level symbId="occasion">
                <ParentLevel>
                    <ct:SymbRef symbIdRef="trial"/>
                </ParentLevel>
            </Level>
        </VariabilityModel>
        <VariabilityModel blkId="obs" type="error">
            <Level symbId="residual"/>
        </VariabilityModel>
        <!-- COVARIATE MODEL -->
        <CovariateModel blkId="cm1">
            <Covariate symbId="W">
                <Continuous>
                    <Transformation>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                            <Binop op="divide">
                                <ct:SymbRef symbIdRef="W"/>
                                <ct:Real>70</ct:Real>
                            </Binop>
                        </Equation>
                    </Transformation>
                </Continuous>
            </Covariate>
        </CovariateModel>
        <!-- PARAMETER MODEL -->
        <ParameterModel blkId="pm1">
            <!-- Rsyn -->
            <SimpleParameter symbId="TVRsyn"/>
            <SimpleParameter symbId="omega_Rsyn"/>
            <!-- Rsyn -->
            <RandomVariable symbId="eta_Rsyn">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Rsyn"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="Rsyn">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths">
                        <Binop op="times">
                            <ct:SymbRef symbIdRef="TVRsyn"/>
                            <Uniop op="exp">
                                <ct:SymbRef symbIdRef="eta_Rsyn"/>
                            </Uniop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            <!-- Css - concentration arising from exogenous levodopa prior to each trial  -->
            <SimpleParameter symbId="TVCss"/>
            <SimpleParameter symbId="omega_Css"/>
            <RandomVariable symbId="eta_Css">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Css"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="Css">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="times">
                            <ct:SymbRef symbIdRef="TVCSS"/>
                            <Uniop op="exp">
                                <ct:SymbRef symbIdRef="eta_Css"/>
                            </Uniop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            <!-- CL -->
            <SimpleParameter symbId="TVCL"/>
            <SimpleParameter symbId="omega_CL"/>
            <SimpleParameter symbId="gamma1_CL"/>
            <SimpleParameter symbId="gamma2_CL"/>
            <!-- CL -->
            <RandomVariable symbId="eta_CL">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_CL"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- CL - trial random effect -->
            <RandomVariable symbId="kappa1_CL">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="trial"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="gamma1_CL"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- CL - occasion random effect -->
            <RandomVariable symbId="kappa2_CL">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="occasion"></ct:SymbRef>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="gamma2_CL"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="CL">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="times">
                            <Binop op="times">
                                <ct:SymbRef symbIdRef="TVCL"/>
                                <Binop op="power">
                                    <ct:SymbRef symbIdRef="W"/>
                                    <ct:Real>0.75</ct:Real>
                                </Binop>
                            </Binop>
                            <Uniop op="exp">
                                <Binop op="plus">
                                    <ct:SymbRef symbIdRef="eta_CL"/>
                                    <Binop op="plus">
                                        <ct:SymbRef symbIdRef="gamma1_CL"/>
                                        <ct:SymbRef symbIdRef="kappa2_CL"/>
                                    </Binop>
                                </Binop>
                            </Uniop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            <!-- Q also known as CL_ic -->
            <!-- Q -->
            <SimpleParameter symbId="TVQ"/>
            <SimpleParameter symbId="omega_Q"/>
            <SimpleParameter symbId="gamma1_Q"/>
            <SimpleParameter symbId="gamma2_Q"/>
            <SimpleParameter symbId="beta_Q_W"/>
            <!-- Q - indiv random effect -->
            <RandomVariable symbId="eta_Q">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_Q"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- Q - trial random effect -->
            <RandomVariable symbId="kappa1_Q">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="trial"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="gamma1_Q"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- Q - occasion random effect -->
            <RandomVariable symbId="kappa2_Q">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="occasion"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="gamma2_Q"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- Q - covariate 'W' -->
            <IndividualParameter symbId="Q">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="times">
                            <Binop op="times">
                                <ct:SymbRef symbIdRef="TVQ"/>
                                <Binop op="power">
                                    <ct:SymbRef symbIdRef="W"/>
                                    <ct:Real>0.75</ct:Real>
                                </Binop>
                            </Binop>
                            <Uniop op="exp">
                                <Binop op="plus">
                                    <ct:SymbRef symbIdRef="eta_Q"/>
                                    <Binop op="plus">
                                        <ct:SymbRef symbIdRef="kappa1_Q"/>
                                        <ct:SymbRef symbIdRef="kappa2_Q"/>
                                    </Binop>
                                </Binop>
                            </Uniop>                            
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            <!-- V1 -->
            <!-- V1 -->
            <SimpleParameter symbId="TVV1"/>
            <SimpleParameter symbId="omega_V1"/>
            <SimpleParameter symbId="gamma1_V1"/>
            <SimpleParameter symbId="gamma2_V1"/>
            <!-- V1 - indiv random effect -->
            <RandomVariable symbId="eta_V1">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_V1"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- V1 - trial random effect -->
            <RandomVariable symbId="kappa1_V1">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="trial"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="gamma1_V1"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- V1 - occasion random effect -->
            <RandomVariable symbId="kappa2_V1">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="occasion"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="gamma2_V1"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- V1 - covariate 'W' -->
            <IndividualParameter symbId="V1">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="times">
                            <ct:SymbRef symbIdRef="TVV1"/>
                            <Binop op="times">
                                <Binop op="power">
                                    <ct:SymbRef blkIdRef="cm1" symbIdRef="W"/>
                                    <ct:SymbRef symbIdRef="beta_V1"/>
                                </Binop>
                                <Uniop op="exp">
                                    <Binop op="plus">
                                        <ct:SymbRef symbIdRef="eta_V1"/>
                                        <Binop op="plus">
                                            <ct:SymbRef symbIdRef="kappa1_V1"/>
                                            <ct:SymbRef symbIdRef="kappa2_V1"/>
                                        </Binop>
                                    </Binop>
                                </Uniop>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            <!-- V2 -->
            <SimpleParameter symbId="TVV2"/>
            <SimpleParameter symbId="omega_V2"/>
            <SimpleParameter symbId="gamma1_V2"/>
            <SimpleParameter symbId="gamma2_V2"/>
            <!-- V2 - indiv random effect -->
            <RandomVariable symbId="eta_V2">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_V2"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- V2 - trial random effect -->
            <RandomVariable symbId="kappa1_V2">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="trial"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="gamma1_V2"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- V2 - occasion random effect -->
            <RandomVariable symbId="kappa2_V2">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="occasion"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_V2"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <!-- V2 - covariate 'W' -->
            <IndividualParameter symbId="V2">
                <!-- V1 = TVV2 * h(W) *exp(eta + kappa1 + kappa2) -->
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="times">
                            <Binop op="power">
                                <ct:SymbRef blkIdRef="cm1" symbIdRef="W"/>
                                <ct:SymbRef symbIdRef="TVV2"/>
                            </Binop>
                            <Uniop op="exp">
                                <Binop op="plus">
                                    <ct:SymbRef symbIdRef="eta_V2"/>
                                    <Binop op="plus">
                                        <ct:SymbRef symbIdRef="kappa1_V2"/>
                                        <ct:SymbRef symbIdRef="kappa2_V2"/>
                                    </Binop>
                                </Binop>
                            </Uniop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            <!-- TTK0 :  -->
            <SimpleParameter symbId="TVTTK0">
                <ct:Description>The typical value. Fixed at 2h </ct:Description>
                <ct:Assign>
                    <ct:Real>2</ct:Real>
                </ct:Assign>
            </SimpleParameter> 
            <SimpleParameter symbId="omega_TTK0"/>
            <RandomVariable symbId="eta_TTK0">
                <ct:VariabilityReference>
                    <ct:SymbRef blkIdRef="model" symbIdRef="indiv"/>
                </ct:VariabilityReference>
                <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                    <mean><rVal>0</rVal></mean>
                    <stddev><var varId="omega_TTK0"/></stddev>
                </NormalDistribution>
            </RandomVariable>
            <IndividualParameter symbId="TTK0">
                <ct:Description>
                    This is the duration of the exogenous infusion
                </ct:Description>
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="times">
                            <ct:SymbRef symbIdRef="TVTTK0"/>
                            <Uniop op="exp">
                                <ct:SymbRef symbIdRef="eta_TTK0"/>
                            </Uniop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </IndividualParameter>
            <SimpleParameter symbId="R1">
                <ct:Description>
                    R1 - Rate of SS infusion: pre-trial dosing
                </ct:Description>
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <ct:SymbRef symbIdRef="CL"/>
                        <ct:SymbRef symbIdRef="Css"/>
                    </Equation>
                </ct:Assign>
            </SimpleParameter> 
        </ParameterModel>
        <!-- STRUCTURAL MODEL -->
        <StructuralModel blkId="sm1">
            <ct:DerivativeVariable symbId="Ac" symbolType="real">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="minus">
                            <Binop op="minus">
                                <Binop op="times">
                                    <Binop op="divide">
                                        <ct:SymbRef symbIdRef="Q"/>
                                        <ct:SymbRef symbIdRef="V2"/>
                                    </Binop>
                                    <ct:SymbRef symbIdRef="Ap"/>
                                </Binop>
                                <Binop op="times">
                                    <Binop op="divide">
                                        <ct:SymbRef symbIdRef="Q"/>
                                        <ct:SymbRef symbIdRef="V1"/>
                                    </Binop>
                                    <ct:SymbRef symbIdRef="Ac"/>
                                </Binop>
                            </Binop>
                            <Binop op="times">
                                <ct:SymbRef symbIdRef="k"/>
                                <ct:SymbRef symbIdRef="Ac"/>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:DerivativeVariable>
            <ct:DerivativeVariable symbId="Ap" symbolType="real">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="minus">
                            <Binop op="times">
                                <Binop op="divide">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="Q"/>
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="V1"/>
                                </Binop>
                                <ct:SymbRef symbIdRef="Ac"/>
                            </Binop>
                            <Binop op="times">
                                <Binop op="divide">
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="Q"/>
                                    <ct:SymbRef blkIdRef="pm1" symbIdRef="V1"/>
                                </Binop>
                                <ct:SymbRef symbIdRef="Ap"/>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:DerivativeVariable>
            <ct:Variable symbolType="real" symbId="Cex">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="divide">
                            <ct:SymbRef symbIdRef="Ac"/>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="V1"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:Variable>
            <ct:Variable symbolType="real" symbId="R1">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="times">
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="CL"/>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="Css"/>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:Variable>
            <ct:Variable symbolType="real" symbId="CP">
                <ct:Assign>
                    <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                        <Binop op="plus">
                            <ct:SymbRef symbIdRef="Cex"/>
                            <Binop op="divide">
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="Rsyn"/>
                                <ct:SymbRef blkIdRef="pm1" symbIdRef="CL"/>
                            </Binop>
                        </Binop>
                    </Equation>
                </ct:Assign>
            </ct:Variable>
        </StructuralModel>
        <!-- OBSERVATION MODEL -->
        <ObservationModel blkId="om1">
            <!-- Observed value with residual error added -->
                <RandomVariable symbId="err1">
                    <ct:VariabilityReference>
                        <ct:SymbRef blkIdRef="obs" symbIdRef="residual"/>
                    </ct:VariabilityReference>
                    <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                        <mean><rVal>0</rVal></mean>
                        <stddev><prVal>1</prVal></stddev>
                    </NormalDistribution>
                </RandomVariable>
                <RandomVariable symbId="err2">
                    <ct:VariabilityReference>
                        <ct:SymbRef blkIdRef="obs" symbIdRef="residual"/>
                    </ct:VariabilityReference>
                    <NormalDistribution xmlns="http://www.uncertml.org/3.0" definition="">
                        <mean><rVal>0</rVal></mean>
                        <stddev><prVal>1</prVal></stddev>
                    </NormalDistribution>
                </RandomVariable>
                <General symbId="CP">
                    <ct:Assign>
                        <Equation xmlns="http://www.pharmml.org/2013/03/Maths" >
                            <Binop op="plus">
                                <Binop op="times">
                                    <ct:SymbRef blkIdRef="sm1" symbIdRef="CP"/>
                                    <Binop op="plus">
                                        <ct:Real>1</ct:Real>
                                        <ct:SymbRef symbIdRef="err1"/>
                                    </Binop>
                                </Binop>
                                <ct:SymbRef symbIdRef="err2"/>
                            </Binop>
                        </Equation>
                    </ct:Assign>
                </General>
        </ObservationModel>
    </ModelDefinition>
    <TrialDesign xmlns="http://www.pharmml.org/2013/03/TrialDesign">
        <Structure>
            <Epoch oid="m1">
                <Start>
                    <ct:Real>0</ct:Real>
                </Start>
                <End>
                    <ct:Real>90</ct:Real>
                </End>
                <Order>1</Order>
            </Epoch>
            <Epoch oid="m6">
                <Start>
                    <ct:Real>408</ct:Real>
                </Start>
                <End>
                    <ct:Real>499</ct:Real>
                </End>
                <Order>2</Order>
            </Epoch>
            <Epoch oid="m12">
                <Start>
                    <ct:Real>908</ct:Real>
                </Start>
                <End>
                    <ct:Real>9999</ct:Real>
                </End>
                <Order>3</Order>
            </Epoch>
            <Epoch oid="m24">
                <Start>
                    <ct:Real>1908</ct:Real>
                </Start>
                <End>
                    <ct:Real>1999</ct:Real>
                </End>
                <Order>4</Order>
            </Epoch>
            <Epoch oid="m48">
                <Start>
                    <ct:Real>3908</ct:Real>
                </Start>
                <End>
                    <ct:Real>3999</ct:Real>
                </End>
                <Order>5</Order>
            </Epoch>
            <Arm oid="a1"/>
            <Cell oid="c1">
                <EpochRef oidRef="m1"/>
                <ArmRef oidRef="a1"/>
                <SegmentRef oidRef="s1"/>
            </Cell>
            <Cell oid="c2">
                <EpochRef oidRef="m6"/>
                <ArmRef oidRef="a1"/>
                <SegmentRef oidRef="s2"/>
            </Cell>
            <Cell oid="c4">
                <EpochRef oidRef="m12"/>
                <ArmRef oidRef="a1"/>
                <SegmentRef oidRef="s2"/>
            </Cell>
            <Cell oid="c5">
                <EpochRef oidRef="m24"/>
                <ArmRef oidRef="a1"/>
                <SegmentRef oidRef="s2"/>
            </Cell>
            <Cell oid="c6">
                <EpochRef oidRef="m48"/>
                <ArmRef oidRef="a1"/>
                <SegmentRef oidRef="s2"/>
            </Cell>
            <Segment oid="s1">
                <ActivityRef oidRef="exoinf"/>
            </Segment>
            <Segment oid="s2">
                <ActivityRef oidRef="exoinf"/>
                <ActivityRef oidRef="oralss"/>
            </Segment>
            <Activity oid="exoinf">
                <Infusion>
                    <DoseAmount inputType="target">
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="Ac"></ct:SymbRef>
                    </DoseAmount>
                    <Duration>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="pm1" symbIdRef="TTK0"/>
                        </ct:Assign>
                    </Duration>
                </Infusion>
            </Activity>
            <Activity oid="oralss">
                <Infusion>
                    <DoseAmount inputType="target">
                        <ct:SymbRef blkIdRef="sm1" symbIdRef="Ac"></ct:SymbRef>
                        <ct:Assign><ct:SymbRef blkIdRef="pm1" symbIdRef="Css"/></ct:Assign>
                    </DoseAmount>
                    <SteadyState>
                        <EndTime>
                            <ct:Assign>
                                <ct:Real>0</ct:Real>
                            </ct:Assign>
                        </EndTime>
                    </SteadyState>
                    <Rate>
                        <ct:Assign>
                            <ct:SymbRef blkIdRef="sm1" symbIdRef="R1"/>
                        </ct:Assign>
                    </Rate>
                </Infusion>
            </Activity>
            <ObservationsEvent oid="occ">
                <ArmRef oidRef="a1"/>
                <ct:Name>Occasions</ct:Name>
                <ct:VariabilityReference>
                    <ct:SymbRef symbIdRef="occasion"/>
                </ct:VariabilityReference>
                <ObservationGroup oid="occ1">
                   <Period> <Start>
                        <ct:Real>0</ct:Real>
                    </Start>
                    <End>
                        <ct:Real>24</ct:Real>
                    </End></Period>
                </ObservationGroup>
                <ObservationGroup oid="occ2">
                    <Period><Start>
                        <ct:Real>72</ct:Real>
                    </Start>
                    <End>
                        <ct:Real>90</ct:Real>
                    </End></Period>
                </ObservationGroup>
                <ObservationGroup oid="occ3">
                    <Period><Start>
                        <ct:Real>408</ct:Real>
                    </Start>
                    <End>
                        <ct:Real>457</ct:Real>
                    </End></Period>
                </ObservationGroup>
                <ObservationGroup oid="occ4">
                    <Period><Start>
                        <ct:Real>481</ct:Real>
                    </Start>
                    <End>
                        <ct:Real>499</ct:Real>
                    </End></Period>
                </ObservationGroup>
                <ObservationGroup oid="occ5">
                   <Period> <Start>
                        <ct:Real>908</ct:Real>
                    </Start>
                    <End>
                        <ct:Real>957</ct:Real>
