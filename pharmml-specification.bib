	Doi = {10.1152/physiolgenomics.00210.2006},
	Journal = {Physiol Genomics},
	Journal-Full = {Physiological genomics},
	Mesh = {Analysis of Variance; Animal Feed; Animals; Autoantigens; Caseins; Chromosome Mapping; Copper; Diabetes Mellitus, Experimental; Diet; Disease Models, Animal; Genetic Testing; Lod Score; Pancreas; Quantitative Trait Loci; Rats},
	Month = {Apr},
