	Mesh = {Antineoplastic Agents; Cell Division; Cell Line, Tumor; Computer Simulation; Data Interpretation, Statistical; Dose-Response Relationship, Drug; Enzyme Inhibitors; Female; Flow Cytometry; G1 Phase; Humans; Mitosis; Models, Biological; Ovarian Neoplasms; S Phase; Topotecan},
	Month = {Apr},
