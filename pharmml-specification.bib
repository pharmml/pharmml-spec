	Mesh = {Analysis of Variance; Blood Glucose; Body Mass Index; Diabetes Mellitus, Type 2; Female; Glucose Clamp Technique; Glucose Intolerance; Glucose Tolerance Test; Humans; Infusions, Intravenous; Insulin; Kinetics; Male; Middle Aged; Models, Biological; Reference Values; Regression Analysis; Sensitivity and Specificity},
	Month = {Sep},
