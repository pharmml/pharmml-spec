	Number = {6},
	Pages = {929-37},
	Pmc = {PMC3112244},
	Pmid = {21471284},
	Pst = {ppublish},
	Title = {Overexpression of mitotic centromere-associated Kinesin stimulates microtubule detachment and confers resistance to paclitaxel},
	Volume = {10},
	Year = {2011},
	Bdsk-Url-1 = {http://dx.doi.org/10.1158/1535-7163.MCT-10-1109}}

