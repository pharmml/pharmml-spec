	Mesh = {Humans; Logistic Models; Markov Chains; Models, Biological; Multicenter Studies as Topic; Multivariate Analysis; Polysomnography; Predictive Value of Tests; Randomized Controlled Trials as Topic; Reproducibility of Results; Sleep Initiation and Maintenance Disorders; Sleep Stages; Time Factors},
	Month = {Sep},
