	Doi = {10.1038/oby.2008.496},
	Journal = {Obesity (Silver Spring)},
	Journal-Full = {Obesity (Silver Spring, Md.)},
	Mesh = {Adolescent; Blood Glucose; C-Peptide; Female; Glucose; Glucose Tolerance Test; Humans; Insulin; Insulin-Secreting Cells; Male; Models, Biological; Time Factors},
