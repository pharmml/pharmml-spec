	Doi = {10.1016/j.bbabio.2010.11.006},
	Journal = {Biochim Biophys Acta},
	Journal-Full = {Biochimica et biophysica acta},
	Month = {Nov},
	Pmid = {21110941},
	Pst = {aheadofprint},
	Title = {Modeling cancer glycolysis},
	Year = {2010},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.bbabio.2010.11.006}}

