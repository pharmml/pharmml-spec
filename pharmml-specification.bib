	Month = {May},
	Number = {5 Pt 1},
	Pages = {E591-8},
	Pmid = {3518490},
	Pst = {ppublish},
	Title = {Estimation of insulin sensitivity and glucose clearance from minimal model: new insights from labeled IVGTT},
	Volume = {250},
