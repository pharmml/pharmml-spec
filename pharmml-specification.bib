	Mesh = {Apoptosis; Cell Line, Tumor; Cyclin-Dependent Kinase Inhibitor p21; Gene Duplication; Humans; Mitosis; Neoplasms; Phosphorylation; Piperazines; Protein-Serine-Threonine Kinases; RNA, Small Interfering; Retinoblastoma Protein; Tumor Suppressor Protein p53},
	Month = {Aug},
