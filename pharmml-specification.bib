	Journal = {J Clin Endocrinol Metab},
	Journal-Full = {The Journal of clinical endocrinology and metabolism},
	Mesh = {Adult; Aged; Blood Glucose; Diabetes Mellitus, Type 2; Fasting; Female; Glucose Clamp Technique; Homeostasis; Humans; Insulin Resistance; Male; Middle Aged},
